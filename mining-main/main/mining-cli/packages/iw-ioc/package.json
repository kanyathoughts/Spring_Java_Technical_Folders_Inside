  "repository": {
    "type": "git",
    "url": "https://github.com/Vortex375/iw-ioc.git"
  },
  "scripts": {
    "build": "rimraf dist && npm run build-ts",
    "clean": "rimraf dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
