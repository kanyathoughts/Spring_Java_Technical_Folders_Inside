    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.161",
    "@types/node": "^13.13.2",
    "concurrently": "^5.1.0",
    "rimraf": "^3.0.2",
    "ts-essentials": "^7.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "main": "dist/index.js"
}
