    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "awilix": "^4.2.6",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13"
  },
