REPEAT WHILE NOT #A OR #B AND #C
  ADD 1 TO #X                                                           
  WRITE NOTITLE '=' #X                                                  
END-REPEAT

REPEAT
  ADD 1 TO #X                                                           
  WRITE NOTITLE '=' #X
  WHILE #B OR #X <= 5 AND #X <= 5 AND #X = 5 OR #B AND #C LE 5
END-REPEAT
*                                                                       
SKIP 3
                                                                  
REPEAT                                                             
  ADD 1 TO #Y                                                           
  WRITE '=' #Y
  UNTIL #C AND #Y = 6 or #Y < 6 OR #Z                                                          
END-REPEAT

REPEAT UNTIL #D OR #Y = 6 or #Y < 6 OR #Z
  ADD 1 TO #Y                                                           
  WRITE '=' #Y
END-REPEAT
*                                                                       
END