       PUT SKIP LIST('PROC_LONG');
       PUT SKIP LIST(PARAM1);
       PUT SKIP LIST(PARAM2);
       
       RETURN ;
    END PROC_LONG;
    
    PROC_OTHER: PROC(PARAM1, PARAM2);
