DEFINE DATA
LOCAL
1 #FLD (A10)
1 #CURSOR (I4)
END-DEFINE

FORMAT PS=100
FORMAT (0) PS=100
FORMAT (1) PS=100

WRITE 'THIS GOES TO UI'
WRITE (0) 'THIS GOES TO UI'
WRITE (1) 'THIS DOES NOT GO TO UI'

PRINT 'THIS GOES TO UI'
PRINT (0) 'THIS GOES TO UI'
PRINT (1) 'THIS DOES NOT GO TO UI'

DISPLAY 'THIS GOES TO UI'
DISPLAY (0) 'THIS GOES TO UI'
DISPLAY (1) 'THIS DOES NOT GO TO UI'

INPUT USING MAP 'WMIN1206M'
INPUT #FLD
INPUT 'CONSTANT VALUE' #FLD
INPUT WINDOW = 'MY-WND' '-' #FLD (AD=OI)
