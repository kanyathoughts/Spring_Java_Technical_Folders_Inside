<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.ui.startup">
		<startup class="innowake.mining.job.plugin.Startup"/>
	</extension>
	<extension point="innowake.mining.plugin.remoteJobController">
		<listener class="innowake.mining.job.plugin.manager.RemoteJobController"/>
	</extension>
	<extension point="org.eclipse.ui.views">
		<view
			allowMultiple="false"
			category="innowake.mining"
			class="innowake.mining.job.plugin.view.JobView"
			icon="icons/progress.png"
			id="innowake.mining.job.jobView"
			name="Job View"
			restorable="true">
		</view>
	</extension>
</plugin>
