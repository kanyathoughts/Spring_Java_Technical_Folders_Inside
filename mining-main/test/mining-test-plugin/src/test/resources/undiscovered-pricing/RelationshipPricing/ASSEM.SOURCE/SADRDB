**********************************************************************  00015000
* SQI COMMUNICATION AREA DSECT ADDRESSED BY REGISTER 11                 00016000
**********************************************************************  00017000
*                                                                       00018000
COM#AREA DSECT                         SQI COMMUNICATION AREA           00019000
SQW@CAF  DS    F                       ADDRESS OF CALL ATTACH FACILITY  00020000
SQW@RET  DS    F                       RETURN TO THIS SQI ADDRESS       00021000
SQWSQLCA DS    XL136                   SQLCA FOR I/O MODULE USE         00022000
         ORG   SQWSQLCA                                                 00023000
         EXEC  SQL INCLUDE SQLCA                                        00024000
SQWSQLDL DS    F                       LENGTH OF DYNAMIC SQLDSECT       00025000
SQWSQLD  DS    F                       DYNAMIC SQLDSECT                 00026000
SQW@SQRX DS    F                       SQI INTERFACE ROUTINE ADDRESS    00027000
SQWPARM1 DS    F                       SQI PARAMETER 1 (MODULE ADDRESS) 00028000
SQWINTHV DS    F                       INTEGER HOST VARIABLE            00029000
SQWCSRBR DS    F                       CURSOR ROUTINE BASE REGISTER     00030000
SQWCSRRA DS    F                       CURSOR ROUTINE RETURN ADDRESS    00031000
SQWRSFVC DS    H                       ROWSET FETCH SQLVAR COUNT        00032000
SQWKMRP  DS    CL1                     SQI KEY MASK / ROUTINE POINTER   00033000
         DS    CL5                     RESERVED                         00034000
SQWIOSI  DS    0CL24                   I/O MODULE STATE INFORMATION     00035000
SQWSQLDA DS    F                       ROWSET SQLDA STORAGE ADDRESS     00036000
SQWRSRMX DS    H                       ROWSET MAX NUMBER OF ROWS        00037000
SQWRSRCT DS    H                       ROWSET RETURNED ROW COUNT        00038000
SQWRSROC DS    H                       ROWSET ROW OCCURS COUNT          00039000
SQWRSFCD DS    H                       ROWSET FETCH SQLCODE             00040000
SQWCSRCA DS    F                       READ CURSOR CLOSE ROUTINE ADDR   00041000
SQWCSUCA DS    F                       UPDATE CURSOR CLOSE ROUTINE ADDR 00042000
SQWCSRLV DS    CL1                     CURSOR CASCADE LEVEL LIMIT       00043000
SQWCSRCC DS    CL1                     CURSOR CASCADE COUNT             00044000
SQWCSRSP DS    H                       CURSOR ROUTINE POINTER           00045000
         DS    CL4                     RESERVED                         00046000
SQWCMD   DS    CL2                     DBS COMMAND CODE                 00047000
SQWORG   DS    CL6                     RECORD ORG CODE                  00048000
ASMREC   DS    0F                      ASSEMBLER RECORD (ALIGNED)       00049000
INST1    DS    CL4                                                      00050000
PRDCDE1  DS    CL6                                                      00051000
ACCTNBR1 DS    CL18                                                     00052000
MODEL1   DS    CL10                                                     00053000
INST2    DS    CL4                                                      00054000
PRDCDE2  DS    CL6                                                      00055000
ACCTNBR2 DS    CL18                                                     00056000
AUDDATE  DS    PL5'0.'                                                  00057000
AUDTIME  DS    PL5'0.'                                                  00058000
AUDUSER  DS    CL8                                                      00059000
AUDORG   DS    CL6                                                      00060000
ACCTST   DS    CL1                                                      00061000
STATUS   DS    CL2                                                      00062000
BRANCH   DS    CL5                                                      00063000
ACCTTYPE DS    CL3                                                      00064000
TERM     DS    CL5                                                      00065000
AUTODEBT DS    CL1                                                      00066000
DLNQDAY  DS    CL3                                                      00067000
RCODE    DS    CL1                                                      00068000
CRDTRATG DS    CL5                                                      00069000
DORMCODE DS    CL1                                                      00070000
BLKCODE  DS    CL1                                                      00071000
RAREA1   DS    CL25                                                     00072000
OFCR1    DS    CL9                                                      00073000
OFCR2    DS    CL9                                                      00074000
COSTCNTR DS    CL5                                                      00075000
SVCNEXT  DS    PL5'0.'                                                  00076000
UOPT1    DS    CL1                                                      00077000
UOPT2    DS    CL1                                                      00078000
UOPT3    DS    CL1                                                      00079000
UOPT4    DS    CL1                                                      00080000
RAREA2   DS    CL21                                                     00081000
CURRBAL  DS    PL8'0.00'                                                00082000
CURRCBAL DS    PL8'0.00'                                                00083000
CURRBBAL DS    PL8'0.00'                                                00084000
LOWBAL   DS    PL8'0.00'                                                00085000
AVGBAL   DS    PL8'0.00'                                                00086000
AVGCBAL  DS    PL8'0.00'                                                00087000
CBAL     DS    PL8'0.00'                                                00088000
CRLIMT   DS    PL8'0.00'                                                00089000
RSVBAL1  DS    PL8'0.00'                                                00090000
RSVBAL2  DS    PL8'0.00'                                                00091000
RSVBAL3  DS    PL8'0.00'                                                00092000
RSVBAL4  DS    PL8'0.00'                                                00093000
RSVBAL5  DS    PL8'0.00'                                                00094000
RSVBAL6  DS    PL8'0.00'                                                00095000
RSVBAL7  DS    PL8'0.00'                                                00096000
USRBAL1  DS    PL8'0.00'                                                00097000
USRBAL2  DS    PL8'0.00'                                                00098000
USRBAL3  DS    PL8'0.00'                                                00099000
USRBAL4  DS    PL8'0.00'                                                00100000
USRBAL5  DS    PL8'0.00'                                                00101000
RAREA3   DS    CL23                                                     00102000
*                                                                       00103000
         ORG   ASMREC+(2000-L'SQWADATA) POINT TO ADDITIONAL DATA        00104000
SQWADATA DS    0CL400                  ADDITIONAL DATA PASSED TO MODULE 00105000
SQWSEGF  DS    CL102                   SEGMENTED FROM KEY VALUE         00106000
SQWSEGT  DS    CL102                   SEGMENTED TO KEY VALUE           00107000
SQWAUDIT DS    CL99                    CALLERS AUDIT DATA               00108000
         DS    CL97                    RESERVED                         00109000
*                                                                       00110000
INDVARS  DS    0H                      NULL INDICATOR VARIABLES         00111000
INDVARX  DS    0H                                                       00112000
INDVARL  EQU   INDVARX-INDVARS         NULL INDICATOR AREA LENGTH       00113000
*                                                                       00114000
*                                                                       00115000
**********************************************************************  00116000
* ROWSET SQLDA AREA ADDRESSED BY REGISTER 2                             00117000
**********************************************************************  00118000
*                                                                       00119000
SQDSQLDA DSECT                         ROWSET SQLDA AREA                00120000
*                                                                       00121000
**********************************************************************  00122000
* PROGRAM TABLE HEADER SECTION:                                         00123000
*   THIS SECTION CONTAINS STATIC DESCRIPTIVE FIELDS.                    00124000
**********************************************************************  00125000
*                                                                       00126000
SADRDB   CSECT                         PROGRAM TABLE SECTION            00127000
SADRDB   AMODE ANY                                                      00128000
SADRDB   RMODE ANY                                                      00129000
         DC    CL8'SADRDB  '           PROGRAM ID                       00130000
         DC    CL1' '                                                   00131000
         DC    CL8'&SYSDATE'           ASSEMBLY DATE                    00132000
         DC    CL1' '                                                   00133000
         DC    CL5'&SYSTIME'           ASSEMBLY TIME                    00134000
         DC    CL1' '                                                   00135000
         DC    A(SQLDLEN)              SQLDSECT SIZE                    00136000
         DC    5A(0)                   RESERVED                         00137000
         DC    AL2(0)                  RESERVED                         00138000
         DC    AL2(INDVARL)            NULL INDICATOR AREA LENGTH       00139000
         DC    A(CONVTAB1)             RECORD/HOST CONVERSION TABLE     00140000
         DC    A(CONVTAB2)             SQLDA DATA TYPE/LENGTH TABLE     00141000
         DC    A(STM#TAB)              STATEMENT TABLE ADDRESS          00142000
         DC    CL43'COPYRIGHT 2017 INFOR. ALL RIGHTS RESERVED. '        00143000
         DC    CL29'WWW.INFOR.COM                '                      00143001
*                                                                       00144000
**********************************************************************  00145000
* STATEMENT TABLE SECTION:                                              00146000
*   THIS SECTION DEFINES AN ENTRY VECTOR FOR EACH SQL STATEMENT.        00147000
**********************************************************************  00148000
*                                                                       00149000
STM#TAB  CSECT                         STATEMENT TABLE SECTION          00150000
STM#TAB  AMODE ANY                                                      00151000
STM#TAB  RMODE ANY                                                      00152000
         DC    A(SELIN0)               SELECT INTO (KEY 0)              00153000
         DC    A(SELIN1)               SELECT INTO (KEY 1)              00154000
         DC    A(SELIN2)               SELECT INTO (KEY 2)              00155000
         DC    A(SELIN3)               SELECT INTO (KEY 3)              00156000
         DC    12A(0)                  UDB MODULE VECTORS               00157000
         DC    A(SELXC0)               SELECT SEQ CURSOR (KEY 0)        00158000
         DC    A(SELXC1)               SELECT SEQ CURSOR (KEY 1)        00159000
         DC    A(SELXC2)               SELECT SEQ CURSOR (KEY 2)        00160000
         DC    A(SELXC3)               SELECT SEQ CURSOR (KEY 3)        00161000
         DC    A(FETDC0)               FETCH SEQ DATA CURSOR (KEY 0)    00162000
         DC    A(FETDC1)               FETCH SEQ DATA CURSOR (KEY 1)    00163000
         DC    A(FETDC2)               FETCH SEQ DATA CURSOR (KEY 2)    00164000
         DC    A(FETDC3)               FETCH SEQ DATA CURSOR (KEY 3)    00165000
         DC    A(FETKC0)               FETCH SEQ KEY CURSOR (KEY 0)     00166000
         DC    A(FETKC1)               FETCH SEQ KEY CURSOR (KEY 1)     00167000
         DC    A(FETKC2)               FETCH SEQ KEY CURSOR (KEY 2)     00168000
         DC    A(FETKC3)               FETCH SEQ KEY CURSOR (KEY 3)     00169000
         DC    A(CLSXC0)               CLOSE SEQ CURSOR (KEY 0)         00170000
         DC    A(CLSXC1)               CLOSE SEQ CURSOR (KEY 1)         00171000
         DC    A(CLSXC2)               CLOSE SEQ CURSOR (KEY 2)         00172000
         DC    A(CLSXC3)               CLOSE SEQ CURSOR (KEY 3)         00173000
         DC    A(SELKY0)               SELECT KEY (KEY 0)               00174000
         DC    A(SELKY1)               SELECT KEY (KEY 1)               00175000
         DC    A(SELKY2)               SELECT KEY (KEY 2)               00176000
         DC    A(SELKY3)               SELECT KEY (KEY 3)               00177000
         DC    10A(0)                  UDB MODULE VECTORS               00178000
         DC    4X'FF'                                                   00179000
*                                                                       00180000
**********************************************************************  00181000
* SQL STATEMENT SECTION:                                                00182000
*   THIS SECTION CONTAINS ALL THE STATIC SQL STATEMENTS REQUIRED        00183000
*     TO SUPPORT THIS TABLE.                                            00184000
*   THE INDICATED STATEMENTS MAY BE MODIFIED, AS LONG AS THE RESULTS    00185000
*     ARE EQUIVALENT.                                                   00186000
**********************************************************************  00187000
*                                                                       00188000
SQL#STMT CSECT                         SQL STATEMENT SECTION            00189000
SQL#STMT AMODE ANY                                                      00190000
SQL#STMT RMODE ANY                                                      00191000
         USING SQDSQLDA,2              ADDRESS ROWSET SQLDA AREA        00192000
         USING SQLDSECT,10,3           ADDRESS SQLDSECT                 00193000
         USING COM#AREA,11             ADDRESS COMMAREA                 00194000
*                                                                       00195000
**********************************************************************  00196000
* SELECT INTO STATEMENT BY PRIMARY KEY:                                 00197000
*   THIS STATEMENT SUPPORTS THE GET (WITHOUT LOCK) VERB.                00198000
**********************************************************************  00199000
*                                                                       00200000
SELIN0   DS    0H                                                       00201000
         USING SELIN0,12               ESTABLISH BASE REGISTER          00202000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00203000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00204000
         BALR  14,15                   MOVE REQUESTED DATA              00205000
         B     *+6                     BRANCH AROUND ADCON              00206000
BASIN0   DC    AL2(4096)                                                00207000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00208000
         AH    3,BASIN0                ADD 4K                           00209000
         EXEC  SQL SELECT                                              *00210000
                   AUDIT_DATE,                                         *00211000
                   AUDIT_TIME,                                         *00212000
                   AUDIT_USER,                                         *00213000
                   AUDIT_ORG,                                          *00214000
                   ACCT_STATUS,                                        *00215000
                   APPL_STATUS,                                        *00216000
                   BRANCH,                                             *00217000
                   ACCT_TYPE,                                          *00218000
                   TERM,                                               *00219000
                   AUTO_DEBIT,                                         *00220000
                   DELINQ_DAY,                                         *00221000
                   RISK_CODE,                                          *00222000
                   CREDIT_RATING,                                      *00223000
                   DORMANT_CODE,                                       *00224000
                   BLOCK_CODE,                                         *00225000
                   RSV_AREA_1,                                         *00226000
                   OFFICER_1,                                          *00227000
                   OFFICER_2,                                          *00228000
                   COST_CENTER,                                        *00229000
                   SVC_NEXT,                                           *00230000
                   USER_OPT_1,                                         *00231000
                   USER_OPT_2,                                         *00232000
                   USER_OPT_3,                                         *00233000
                   USER_OPT_4,                                         *00234000
                   RSV_AREA_2,                                         *00235000
                   CURR_BAL,                                           *00236000
                   CURR_C_BAL,                                         *00237000
                   CURR_B_BAL,                                         *00238000
                   LOW_BAL,                                            *00239000
                   AVG_BAL,                                            *00240000
                   AVG_COL_BAL,                                        *00241000
                   COMMIT_BAL,                                         *00242000
                   CREDIT_LIMIT,                                       *00243000
                   RSV_BAL_1,                                          *00244000
                   RSV_BAL_2,                                          *00245000
                   RSV_BAL_3,                                          *00246000
                   RSV_BAL_4,                                          *00247000
                   RSV_BAL_5,                                          *00248000
                   RSV_BAL_6,                                          *00249000
                   RSV_BAL_7,                                          *00250000
                   USER_BAL_1,                                         *00251000
                   USER_BAL_2,                                         *00252000
                   USER_BAL_3,                                         *00253000
                   USER_BAL_4,                                         *00254000
                   USER_BAL_5,                                         *00255000
                   RSV_AREA_3                                          *00256000
                 INTO                                                  *00257000
                   :AUDDATE,                                           *00258000
                   :AUDTIME,                                           *00259000
                   :AUDUSER,                                           *00260000
                   :AUDORG,                                            *00261000
                   :ACCTST,                                            *00262000
                   :STATUS,                                            *00263000
                   :BRANCH,                                            *00264000
                   :ACCTTYPE,                                          *00265000
                   :TERM,                                              *00266000
                   :AUTODEBT,                                          *00267000
                   :DLNQDAY,                                           *00268000
                   :RCODE,                                             *00269000
                   :CRDTRATG,                                          *00270000
                   :DORMCODE,                                          *00271000
                   :BLKCODE,                                           *00272000
                   :RAREA1,                                            *00273000
                   :OFCR1,                                             *00274000
                   :OFCR2,                                             *00275000
                   :COSTCNTR,                                          *00276000
                   :SVCNEXT,                                           *00277000
                   :UOPT1,                                             *00278000
                   :UOPT2,                                             *00279000
                   :UOPT3,                                             *00280000
                   :UOPT4,                                             *00281000
                   :RAREA2,                                            *00282000
                   :CURRBAL,                                           *00283000
                   :CURRCBAL,                                          *00284000
                   :CURRBBAL,                                          *00285000
                   :LOWBAL,                                            *00286000
                   :AVGBAL,                                            *00287000
                   :AVGCBAL,                                           *00288000
                   :CBAL,                                              *00289000
                   :CRLIMT,                                            *00290000
                   :RSVBAL1,                                           *00291000
                   :RSVBAL2,                                           *00292000
                   :RSVBAL3,                                           *00293000
                   :RSVBAL4,                                           *00294000
                   :RSVBAL5,                                           *00295000
                   :RSVBAL6,                                           *00296000
                   :RSVBAL7,                                           *00297000
                   :USRBAL1,                                           *00298000
                   :USRBAL2,                                           *00299000
                   :USRBAL3,                                           *00300000
                   :USRBAL4,                                           *00301000
                   :USRBAL5,                                           *00302000
                   :RAREA3                                             *00303000
                 FROM SAD                                              *00304000
                 WHERE                                                 *00305000
                   ACCT_INST_1 = :INST1 AND                            *00306000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00307000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00308000
                   MODEL_1 = :MODEL1 AND                               *00309000
                   ACCT_INST_2 = :INST2 AND                            *00310000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00311000
                   ACCT_NBR_2 = :ACCTNBR2                              *00312000
                 FETCH FIRST 1 ROW ONLY                                 00313000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00314000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00315000
         BALR  14,15                   MOVE REQUESTED DATA              00316000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00317000
         BR    14                      RETURN TO CALLER                 00318000
         LTORG                                                          00319000
*                                                                       00320000
**********************************************************************  00321000
* SELECT COUNT STATEMENT BY PRIMARY KEY:                                00322000
*   THIS STATEMENT SUPPORTS THE LOCATE VERB.                            00323000
**********************************************************************  00324000
*                                                                       00325000
SELKY0   DS    0H                                                       00326000
         USING SELKY0,12               ESTABLISH BASE REGISTER          00327000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00328000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00329000
         BALR  14,15                   MOVE REQUESTED DATA              00330000
         B     *+6                     BRANCH AROUND ADCON              00331000
BASKY0   DC    AL2(4096)                                                00332000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00333000
         AH    3,BASKY0                ADD 4K                           00334000
         EXEC  SQL SELECT                                              *00335000
                   COUNT(*)                                            *00336000
                 INTO                                                  *00337000
                   :SQWINTHV                                           *00338000
                 FROM SAD                                              *00339000
                 WHERE                                                 *00340000
                   ACCT_INST_1 = :INST1 AND                            *00341000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00342000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00343000
                   MODEL_1 = :MODEL1 AND                               *00344000
                   ACCT_INST_2 = :INST2 AND                            *00345000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00346000
                   ACCT_NBR_2 = :ACCTNBR2                              *00347000
                 FETCH FIRST 1 ROW ONLY                                 00348000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00349000
         CLC   SQLCODE,=F'0'           GOOD RETURN FROM SQL?            00350000
         BNER  14                      NO - RETURN TO CALLER            00351000
         CLC   SQWINTHV,=F'0'          ANY ROWS MATCH WHERE CLAUSE?     00352000
         BNER  14                      YES - RETURN ZERO SQLCODE        00353000
         MVC   SQLCODE,=F'+100'        SET SQLCODE TO ROW NOT FOUND     00354000
         BR    14                      RETURN TO CALLER                 00355000
         LTORG                                                          00356000
*                                                                       00357000
**********************************************************************  00358000
* SELECT SEQUENTIAL AND OPEN SEQUENTIAL CURSOR STATEMENTS BY PRIMARY    00359000
* KEY:                                                                  00360000
*   THESE STATEMENTS SUPPORT THE GET-GE, GET-GE-LOCK, GET-NEXT,         00361000
*     AND GET-NEXT-LOCK VERBS.                                          00362000
*   A SELECT UPDATE STATEMENT WILL FOLLOW IN THE CASE OF A              00363000
*     GET-GE-LOCK OR GET-NEXT-LOCK VERB.                                00364000
**********************************************************************  00365000
*                                                                       00366000
SELXC0   DS    0H                                                       00367000
         USING SELXC0,12               ESTABLISH BASE REGISTER          00368000
         MVI   SQWKMRP,X'86'           SET HOST KEY 0 & CURSOR POINTER  00369000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00370000
         BALR  14,15                   MOVE DATA & SET CURSOR POINTER   00371000
         LA    12,SELXC0P              LOAD VECTOR TABLE ADDRESS        00372000
         AH    12,SQWCSRSP             COMPUTE POINTER TO OPEN ROUTINE  00373000
         L     12,0(12)                LOAD OPEN ROUTINE ADDRESS        00374000
         BR    12                      GO TO CURSOR OPEN ROUTINE        00375000
SELXC0P  DC    A(SELGE001)                                              00376000
         DC    (KY0COLMS-01)A(0)                                        00377000
         DC    A(SELGE002)                                              00378000
         DC    (KY0COLMS-02)A(0)                                        00379000
         DC    A(SELGE003)                                              00380000
         DC    (KY0COLMS-03)A(0)                                        00381000
         DC    A(SELGE004)                                              00382000
         DC    (KY0COLMS-04)A(0)                                        00383000
         DC    A(SELGE005)                                              00384000
         DC    (KY0COLMS-05)A(0)                                        00385000
         DC    A(SELGE006)                                              00386000
         DC    (KY0COLMS-06)A(0)                                        00387000
         DC    A(SELGE007)                                              00388000
         LTORG                                                          00389000
*                                                                       00390000
SELGE001 DS    0H                                                       00391000
         USING SELGE001,12             ESTABLISH BASE REGISTER          00392000
         B     *+6                     BRANCH AROUND ADCON              00393000
BASGE001 DC    AL2(4096)                                                00394000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00395000
         AH    3,BASGE001              ADD 4K                           00396000
         EXEC  SQL DECLARE SADGE001 CURSOR                             *00397000
               WITH ROWSET POSITIONING                                 *00398000
               FOR SELECT                                              *00399000
                   ACCT_INST_1,                                        *00400000
                   PROD_CODE_1,                                        *00401000
                   ACCT_NBR_1,                                         *00402000
                   MODEL_1,                                            *00403000
                   ACCT_INST_2,                                        *00404000
                   PROD_CODE_2,                                        *00405000
                   ACCT_NBR_2,                                         *00406000
                   AUDIT_DATE,                                         *00407000
                   AUDIT_TIME,                                         *00408000
                   AUDIT_USER,                                         *00409000
                   AUDIT_ORG,                                          *00410000
                   ACCT_STATUS,                                        *00411000
                   APPL_STATUS,                                        *00412000
                   BRANCH,                                             *00413000
                   ACCT_TYPE,                                          *00414000
                   TERM,                                               *00415000
                   AUTO_DEBIT,                                         *00416000
                   DELINQ_DAY,                                         *00417000
                   RISK_CODE,                                          *00418000
                   CREDIT_RATING,                                      *00419000
                   DORMANT_CODE,                                       *00420000
                   BLOCK_CODE,                                         *00421000
                   RSV_AREA_1,                                         *00422000
                   OFFICER_1,                                          *00423000
                   OFFICER_2,                                          *00424000
                   COST_CENTER,                                        *00425000
                   SVC_NEXT,                                           *00426000
                   USER_OPT_1,                                         *00427000
                   USER_OPT_2,                                         *00428000
                   USER_OPT_3,                                         *00429000
                   USER_OPT_4,                                         *00430000
                   RSV_AREA_2,                                         *00431000
                   CURR_BAL,                                           *00432000
                   CURR_C_BAL,                                         *00433000
                   CURR_B_BAL,                                         *00434000
                   LOW_BAL,                                            *00435000
                   AVG_BAL,                                            *00436000
                   AVG_COL_BAL,                                        *00437000
                   COMMIT_BAL,                                         *00438000
