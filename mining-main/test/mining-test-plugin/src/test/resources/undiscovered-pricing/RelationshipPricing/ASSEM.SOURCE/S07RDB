BRREGN27 DS    PL3'0.'                                                  00084000
BRREGN28 DS    PL3'0.'                                                  00085000
BRREGN29 DS    PL3'0.'                                                  00086000
BRREGN30 DS    PL3'0.'                                                  00087000
BRREGN31 DS    PL3'0.'                                                  00088000
BRREGN32 DS    PL3'0.'                                                  00089000
BRREGN33 DS    PL3'0.'                                                  00090000
BRREGN34 DS    PL3'0.'                                                  00091000
BRREGN35 DS    PL3'0.'                                                  00092000
BRREGN36 DS    PL3'0.'                                                  00093000
BRREGN37 DS    PL3'0.'                                                  00094000
BRREGN38 DS    PL3'0.'                                                  00095000
BRREGN39 DS    PL3'0.'                                                  00096000
BRREGN40 DS    PL3'0.'                                                  00097000
BRREGN41 DS    PL3'0.'                                                  00098000
BRREGN42 DS    PL3'0.'                                                  00099000
BRREGN43 DS    PL3'0.'                                                  00100000
BRREGN44 DS    PL3'0.'                                                  00101000
BRREGN45 DS    PL3'0.'                                                  00102000
BRREGN46 DS    PL3'0.'                                                  00103000
BRREGN47 DS    PL3'0.'                                                  00104000
BRREGN48 DS    PL3'0.'                                                  00105000
BRREGN49 DS    PL3'0.'                                                  00106000
BRREGN50 DS    PL3'0.'                                                  00107000
TABLETYP DS    CL1                                                      00108000
*                                                                       00109000
         ORG   ASMREC+(2000-L'SQWADATA) POINT TO ADDITIONAL DATA        00110000
SQWADATA DS    0CL400                  ADDITIONAL DATA PASSED TO MODULE 00111000
SQWSEGF  DS    CL102                   SEGMENTED FROM KEY VALUE         00112000
SQWSEGT  DS    CL102                   SEGMENTED TO KEY VALUE           00113000
SQWAUDIT DS    CL99                    CALLERS AUDIT DATA               00114000
         DS    CL97                    RESERVED                         00115000
*                                                                       00116000
INDVARS  DS    0H                      NULL INDICATOR VARIABLES         00117000
INDVARX  DS    0H                                                       00118000
INDVARL  EQU   INDVARX-INDVARS         NULL INDICATOR AREA LENGTH       00119000
*                                                                       00120000
*                                                                       00121000
**********************************************************************  00122000
* ROWSET SQLDA AREA ADDRESSED BY REGISTER 2                             00123000
**********************************************************************  00124000
*                                                                       00125000
SQDSQLDA DSECT                         ROWSET SQLDA AREA                00126000
*                                                                       00127000
**********************************************************************  00128000
* PROGRAM TABLE HEADER SECTION:                                         00129000
*   THIS SECTION CONTAINS STATIC DESCRIPTIVE FIELDS.                    00130000
**********************************************************************  00131000
*                                                                       00132000
S07RDB   CSECT                         PROGRAM TABLE SECTION            00133000
S07RDB   AMODE ANY                                                      00134000
S07RDB   RMODE ANY                                                      00135000
         DC    CL8'S07RDB  '           PROGRAM ID                       00136000
         DC    CL1' '                                                   00137000
         DC    CL8'&SYSDATE'           ASSEMBLY DATE                    00138000
         DC    CL1' '                                                   00139000
         DC    CL5'&SYSTIME'           ASSEMBLY TIME                    00140000
         DC    CL1' '                                                   00141000
         DC    A(SQLDLEN)              SQLDSECT SIZE                    00142000
         DC    5A(0)                   RESERVED                         00143000
         DC    AL2(0)                  RESERVED                         00144000
         DC    AL2(INDVARL)            NULL INDICATOR AREA LENGTH       00145000
         DC    A(CONVTAB1)             RECORD/HOST CONVERSION TABLE     00146000
         DC    A(CONVTAB2)             SQLDA DATA TYPE/LENGTH TABLE     00147000
         DC    A(STM#TAB)              STATEMENT TABLE ADDRESS          00148000
         DC    CL43'COPYRIGHT 2017 INFOR. ALL RIGHTS RESERVED. '        00149000
         DC    CL29'WWW.INFOR.COM                '                      00149001
*                                                                       00150000
**********************************************************************  00151000
* STATEMENT TABLE SECTION:                                              00152000
*   THIS SECTION DEFINES AN ENTRY VECTOR FOR EACH SQL STATEMENT.        00153000
**********************************************************************  00154000
*                                                                       00155000
STM#TAB  CSECT                         STATEMENT TABLE SECTION          00156000
STM#TAB  AMODE ANY                                                      00157000
STM#TAB  RMODE ANY                                                      00158000
         DC    A(SELIN0)               SELECT INTO (KEY 0)              00159000
         DC    A(SELIN1)               SELECT INTO (KEY 1)              00160000
         DC    A(SELIN2)               SELECT INTO (KEY 2)              00161000
         DC    A(SELIN3)               SELECT INTO (KEY 3)              00162000
         DC    12A(0)                  UDB MODULE VECTORS               00163000
         DC    A(SELXC0)               SELECT SEQ CURSOR (KEY 0)        00164000
         DC    A(SELXC1)               SELECT SEQ CURSOR (KEY 1)        00165000
         DC    A(SELXC2)               SELECT SEQ CURSOR (KEY 2)        00166000
         DC    A(SELXC3)               SELECT SEQ CURSOR (KEY 3)        00167000
         DC    A(FETDC0)               FETCH SEQ DATA CURSOR (KEY 0)    00168000
         DC    A(FETDC1)               FETCH SEQ DATA CURSOR (KEY 1)    00169000
         DC    A(FETDC2)               FETCH SEQ DATA CURSOR (KEY 2)    00170000
         DC    A(FETDC3)               FETCH SEQ DATA CURSOR (KEY 3)    00171000
         DC    A(FETKC0)               FETCH SEQ KEY CURSOR (KEY 0)     00172000
         DC    A(FETKC1)               FETCH SEQ KEY CURSOR (KEY 1)     00173000
         DC    A(FETKC2)               FETCH SEQ KEY CURSOR (KEY 2)     00174000
         DC    A(FETKC3)               FETCH SEQ KEY CURSOR (KEY 3)     00175000
         DC    A(CLSXC0)               CLOSE SEQ CURSOR (KEY 0)         00176000
         DC    A(CLSXC1)               CLOSE SEQ CURSOR (KEY 1)         00177000
         DC    A(CLSXC2)               CLOSE SEQ CURSOR (KEY 2)         00178000
         DC    A(CLSXC3)               CLOSE SEQ CURSOR (KEY 3)         00179000
         DC    A(SELKY0)               SELECT KEY (KEY 0)               00180000
         DC    A(SELKY1)               SELECT KEY (KEY 1)               00181000
         DC    A(SELKY2)               SELECT KEY (KEY 2)               00182000
         DC    A(SELKY3)               SELECT KEY (KEY 3)               00183000
         DC    10A(0)                  UDB MODULE VECTORS               00184000
         DC    4X'FF'                                                   00185000
*                                                                       00186000
**********************************************************************  00187000
* SQL STATEMENT SECTION:                                                00188000
*   THIS SECTION CONTAINS ALL THE STATIC SQL STATEMENTS REQUIRED        00189000
*     TO SUPPORT THIS TABLE.                                            00190000
*   THE INDICATED STATEMENTS MAY BE MODIFIED, AS LONG AS THE RESULTS    00191000
*     ARE EQUIVALENT.                                                   00192000
**********************************************************************  00193000
*                                                                       00194000
SQL#STMT CSECT                         SQL STATEMENT SECTION            00195000
SQL#STMT AMODE ANY                                                      00196000
SQL#STMT RMODE ANY                                                      00197000
         USING SQDSQLDA,2              ADDRESS ROWSET SQLDA AREA        00198000
         USING SQLDSECT,10,3           ADDRESS SQLDSECT                 00199000
         USING COM#AREA,11             ADDRESS COMMAREA                 00200000
*                                                                       00201000
**********************************************************************  00202000
* SELECT INTO STATEMENT BY PRIMARY KEY:                                 00203000
*   THIS STATEMENT SUPPORTS THE GET (WITHOUT LOCK) VERB.                00204000
**********************************************************************  00205000
*                                                                       00206000
SELIN0   DS    0H                                                       00207000
         USING SELIN0,12               ESTABLISH BASE REGISTER          00208000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00209000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00210000
         BALR  14,15                   MOVE REQUESTED DATA              00211000
         B     *+6                     BRANCH AROUND ADCON              00212000
BASIN0   DC    AL2(4096)                                                00213000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00214000
         AH    3,BASIN0                ADD 4K                           00215000
         EXEC  SQL SELECT                                              *00216000
                   AUDIT_DATE,                                         *00217000
                   AUDIT_TIME,                                         *00218000
                   AUDIT_USER,                                         *00219000
                   AUDIT_ORG,                                          *00220000
                   INCL_EXCL_OPT,                                      *00221000
                   BRCH_REGION_01,                                     *00222000
                   BRCH_REGION_02,                                     *00223000
                   BRCH_REGION_03,                                     *00224000
                   BRCH_REGION_04,                                     *00225000
                   BRCH_REGION_05,                                     *00226000
                   BRCH_REGION_06,                                     *00227000
                   BRCH_REGION_07,                                     *00228000
                   BRCH_REGION_08,                                     *00229000
                   BRCH_REGION_09,                                     *00230000
                   BRCH_REGION_10,                                     *00231000
                   BRCH_REGION_11,                                     *00232000
                   BRCH_REGION_12,                                     *00233000
                   BRCH_REGION_13,                                     *00234000
                   BRCH_REGION_14,                                     *00235000
                   BRCH_REGION_15,                                     *00236000
                   BRCH_REGION_16,                                     *00237000
                   BRCH_REGION_17,                                     *00238000
                   BRCH_REGION_18,                                     *00239000
                   BRCH_REGION_19,                                     *00240000
                   BRCH_REGION_20,                                     *00241000
                   BRCH_REGION_21,                                     *00242000
                   BRCH_REGION_22,                                     *00243000
                   BRCH_REGION_23,                                     *00244000
                   BRCH_REGION_24,                                     *00245000
                   BRCH_REGION_25,                                     *00246000
                   BRCH_REGION_26,                                     *00247000
                   BRCH_REGION_27,                                     *00248000
                   BRCH_REGION_28,                                     *00249000
                   BRCH_REGION_29,                                     *00250000
                   BRCH_REGION_30,                                     *00251000
                   BRCH_REGION_31,                                     *00252000
                   BRCH_REGION_32,                                     *00253000
                   BRCH_REGION_33,                                     *00254000
                   BRCH_REGION_34,                                     *00255000
                   BRCH_REGION_35,                                     *00256000
                   BRCH_REGION_36,                                     *00257000
                   BRCH_REGION_37,                                     *00258000
                   BRCH_REGION_38,                                     *00259000
                   BRCH_REGION_39,                                     *00260000
                   BRCH_REGION_40,                                     *00261000
                   BRCH_REGION_41,                                     *00262000
                   BRCH_REGION_42,                                     *00263000
                   BRCH_REGION_43,                                     *00264000
                   BRCH_REGION_44,                                     *00265000
                   BRCH_REGION_45,                                     *00266000
                   BRCH_REGION_46,                                     *00267000
                   BRCH_REGION_47,                                     *00268000
                   BRCH_REGION_48,                                     *00269000
                   BRCH_REGION_49,                                     *00270000
                   BRCH_REGION_50,                                     *00271000
                   TABLE_TYPE                                          *00272000
                 INTO                                                  *00273000
                   :AUDDATE,                                           *00274000
                   :AUDTIME,                                           *00275000
                   :AUDUSER,                                           *00276000
                   :AUDORG,                                            *00277000
                   :INCLEXCL,                                          *00278000
                   :BRREGN01,                                          *00279000
                   :BRREGN02,                                          *00280000
                   :BRREGN03,                                          *00281000
                   :BRREGN04,                                          *00282000
                   :BRREGN05,                                          *00283000
                   :BRREGN06,                                          *00284000
                   :BRREGN07,                                          *00285000
                   :BRREGN08,                                          *00286000
                   :BRREGN09,                                          *00287000
                   :BRREGN10,                                          *00288000
                   :BRREGN11,                                          *00289000
                   :BRREGN12,                                          *00290000
                   :BRREGN13,                                          *00291000
                   :BRREGN14,                                          *00292000
                   :BRREGN15,                                          *00293000
                   :BRREGN16,                                          *00294000
                   :BRREGN17,                                          *00295000
                   :BRREGN18,                                          *00296000
                   :BRREGN19,                                          *00297000
                   :BRREGN20,                                          *00298000
                   :BRREGN21,                                          *00299000
                   :BRREGN22,                                          *00300000
                   :BRREGN23,                                          *00301000
                   :BRREGN24,                                          *00302000
                   :BRREGN25,                                          *00303000
                   :BRREGN26,                                          *00304000
                   :BRREGN27,                                          *00305000
                   :BRREGN28,                                          *00306000
                   :BRREGN29,                                          *00307000
                   :BRREGN30,                                          *00308000
                   :BRREGN31,                                          *00309000
                   :BRREGN32,                                          *00310000
                   :BRREGN33,                                          *00311000
                   :BRREGN34,                                          *00312000
                   :BRREGN35,                                          *00313000
                   :BRREGN36,                                          *00314000
                   :BRREGN37,                                          *00315000
                   :BRREGN38,                                          *00316000
                   :BRREGN39,                                          *00317000
                   :BRREGN40,                                          *00318000
                   :BRREGN41,                                          *00319000
                   :BRREGN42,                                          *00320000
                   :BRREGN43,                                          *00321000
                   :BRREGN44,                                          *00322000
                   :BRREGN45,                                          *00323000
                   :BRREGN46,                                          *00324000
                   :BRREGN47,                                          *00325000
                   :BRREGN48,                                          *00326000
                   :BRREGN49,                                          *00327000
                   :BRREGN50,                                          *00328000
                   :TABLETYP                                           *00329000
                 FROM S07                                              *00330000
                 WHERE                                                 *00331000
                   INST_NBR = :INST AND                                *00332000
                   RECORD_NBR = :RECNBR AND                            *00333000
                   BRCH_REGN_CODE = :BRREGNCD                          *00334000
                 FETCH FIRST 1 ROW ONLY                                 00335000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00336000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00337000
         BALR  14,15                   MOVE REQUESTED DATA              00338000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00339000
         BR    14                      RETURN TO CALLER                 00340000
         LTORG                                                          00341000
*                                                                       00342000
**********************************************************************  00343000
* SELECT COUNT STATEMENT BY PRIMARY KEY:                                00344000
*   THIS STATEMENT SUPPORTS THE LOCATE VERB.                            00345000
**********************************************************************  00346000
*                                                                       00347000
SELKY0   DS    0H                                                       00348000
         USING SELKY0,12               ESTABLISH BASE REGISTER          00349000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00350000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00351000
         BALR  14,15                   MOVE REQUESTED DATA              00352000
         B     *+6                     BRANCH AROUND ADCON              00353000
BASKY0   DC    AL2(4096)                                                00354000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00355000
         AH    3,BASKY0                ADD 4K                           00356000
         EXEC  SQL SELECT                                              *00357000
                   COUNT(*)                                            *00358000
                 INTO                                                  *00359000
                   :SQWINTHV                                           *00360000
                 FROM S07                                              *00361000
                 WHERE                                                 *00362000
                   INST_NBR = :INST AND                                *00363000
                   RECORD_NBR = :RECNBR AND                            *00364000
                   BRCH_REGN_CODE = :BRREGNCD                          *00365000
                 FETCH FIRST 1 ROW ONLY                                 00366000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00367000
         CLC   SQLCODE,=F'0'           GOOD RETURN FROM SQL?            00368000
         BNER  14                      NO - RETURN TO CALLER            00369000
         CLC   SQWINTHV,=F'0'          ANY ROWS MATCH WHERE CLAUSE?     00370000
         BNER  14                      YES - RETURN ZERO SQLCODE        00371000
         MVC   SQLCODE,=F'+100'        SET SQLCODE TO ROW NOT FOUND     00372000
         BR    14                      RETURN TO CALLER                 00373000
         LTORG                                                          00374000
*                                                                       00375000
**********************************************************************  00376000
* SELECT SEQUENTIAL AND OPEN SEQUENTIAL CURSOR STATEMENTS BY PRIMARY    00377000
* KEY:                                                                  00378000
*   THESE STATEMENTS SUPPORT THE GET-GE, GET-GE-LOCK, GET-NEXT,         00379000
*     AND GET-NEXT-LOCK VERBS.                                          00380000
*   A SELECT UPDATE STATEMENT WILL FOLLOW IN THE CASE OF A              00381000
*     GET-GE-LOCK OR GET-NEXT-LOCK VERB.                                00382000
**********************************************************************  00383000
*                                                                       00384000
SELXC0   DS    0H                                                       00385000
         USING SELXC0,12               ESTABLISH BASE REGISTER          00386000
         MVI   SQWKMRP,X'86'           SET HOST KEY 0 & CURSOR POINTER  00387000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00388000
         BALR  14,15                   MOVE DATA & SET CURSOR POINTER   00389000
         LA    12,SELXC0P              LOAD VECTOR TABLE ADDRESS        00390000
         AH    12,SQWCSRSP             COMPUTE POINTER TO OPEN ROUTINE  00391000
         L     12,0(12)                LOAD OPEN ROUTINE ADDRESS        00392000
         BR    12                      GO TO CURSOR OPEN ROUTINE        00393000
SELXC0P  DC    A(SELGE001)                                              00394000
         DC    (KY0COLMS-01)A(0)                                        00395000
         DC    A(SELGE002)                                              00396000
         DC    (KY0COLMS-02)A(0)                                        00397000
         DC    A(SELGE003)                                              00398000
         LTORG                                                          00399000
*                                                                       00400000
SELGE001 DS    0H                                                       00401000
         USING SELGE001,12             ESTABLISH BASE REGISTER          00402000
         B     *+6                     BRANCH AROUND ADCON              00403000
BASGE001 DC    AL2(4096)                                                00404000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00405000
         AH    3,BASGE001              ADD 4K                           00406000
         EXEC  SQL DECLARE S07GE001 CURSOR                             *00407000
               WITH ROWSET POSITIONING                                 *00408000
               FOR SELECT                                              *00409000
                   INST_NBR,                                           *00410000
                   RECORD_NBR,                                         *00411000
                   BRCH_REGN_CODE,                                     *00412000
                   AUDIT_DATE,                                         *00413000
                   AUDIT_TIME,                                         *00414000
                   AUDIT_USER,                                         *00415000
                   AUDIT_ORG,                                          *00416000
                   INCL_EXCL_OPT,                                      *00417000
                   BRCH_REGION_01,                                     *00418000
                   BRCH_REGION_02,                                     *00419000
                   BRCH_REGION_03,                                     *00420000
                   BRCH_REGION_04,                                     *00421000
                   BRCH_REGION_05,                                     *00422000
                   BRCH_REGION_06,                                     *00423000
                   BRCH_REGION_07,                                     *00424000
                   BRCH_REGION_08,                                     *00425000
                   BRCH_REGION_09,                                     *00426000
                   BRCH_REGION_10,                                     *00427000
                   BRCH_REGION_11,                                     *00428000
                   BRCH_REGION_12,                                     *00429000
