**********************************************************************  01865000
* SELECT AND OPEN SEQUENTIAL CURSOR STATEMENTS BY PRIMARY KEY:          01866000
*   THIS ROUTINE HANDLES PRIMARY KEY SEQUENTIAL CURSORS.                01867000
**********************************************************************  01868000
*                                                                       01869000
SQL#0046 DS    0H                                                       01870000
         USING SQL#0046,12             ESTABLISH BASE REGISTER          01871000
         LA    3,X'80'                 LOAD MASK FOR KEY 0              01872000
         L     5,=A(IN#KEY)            LOAD KEY CONVERSION ROUTINE      01873000
         BALR  4,5                     MOVE INTO HOST VARIABLES         01874000
         LH    1,CSRPTR                LOAD CURRENT CURSOR POINTER      01875000
         LA    1,4(1)                  INCREMENT TO NEXT CURSOR         01876000
         STH   1,CSRPTR                SAVE FOR NEXT CALL               01877000
         LA    12,VECT0046(1)          LOAD POINTER TO NEXT CURSOR      01878000
         L     12,0(12)                LOAD CURSOR ROUTINE ADDRESS      01879000
         BR    12                      GO TO CURRENT CURSOR ROUTINE     01880000
VECT0046 DC    A(0)                                                     01881000
         DC    A(SQL#0246)                                              01882000
         DC    A(SQL#0346)                                              01883000
         DC    A(SQL#0446)                                              01884000
         DC    A(SQL#0546)                                              01885000
         DC    A(SQL#0646)                                              01886000
         DC    A(SQL#0746)                                              01887000
         DC    A(SQL#0846)                                              01888000
         DC    A(SQL#0946)                                              01889000
         DC    A(SQL#1046)                                              01890000
         LTORG                                                          01891000
*                                                                       01892000
SQL#0246 DS    0H                                                       01893000
         USING SQL#0246,12             ESTABLISH BASE REGISTER          01894000
         EXEC  SQL                                                     *01895000
               CLOSE SAESQ001                                           01896000
         EXEC  SQL                                                     *01897000
               DECLARE SAESQ002 CURSOR FOR                             *01898000
               SELECT ZERO_NBR,                                        *01899000
                   AUDIT_DATE_KEY,                                     *01900000
                   AUDIT_TIME_KEY,                                     *01901000
                   UNIQUE_CODE,                                        *01902000
                   AUDIT_TIME_SUBS,                                    *01903000
                   TRAN_CODE,                                          *01904000
                   INST_NBR,                                           *01905000
                   FORMAT_ID,                                          *01906000
                   LOG_CODE,                                           *01907000
                   SEGM_ID,                                            *01908000
                   FUNC_CODE,                                          *01909000
                   AUDIT_USER,                                         *01910000
                   RECORD_LENGTH,                                      *01911000
                   TERMINAL_ID,                                        *01912000
                   WORK_ID,                                            *01913000
                   PANEL_ID,                                           *01914000
                   TRAN_BEGIN,                                         *01915000
                   APPLIC_SEQ,                                         *01916000
                   DATA_1,                                             *01917000
                   DATA_2,                                             *01918000
                   DATA_3                                              *01919000
                 FROM SAE                                              *01920000
                 WHERE                                                 *01921000
                    ZERO_NBR = :ZERONBR AND                            *01922000
                    AUDIT_DATE_KEY = :AUDDATK AND                      *01923000
                    AUDIT_TIME_KEY = :AUDTIMK AND                      *01924000
                    UNIQUE_CODE = :UNIQCDE AND                         *01925000
                    AUDIT_TIME_SUBS = :AUDTIMS AND                     *01926000
                    TRAN_CODE = :TRANSCD AND                           *01927000
                    INST_NBR = :INSTNBR AND                            *01928000
                    FORMAT_ID = :FORMATID AND                          *01929000
                    LOG_CODE > :LOGCODE                                *01930000
                 ORDER BY LOG_CODE,                                    *01931000
                   SEGM_ID                                             *01932000
                 OPTIMIZE FOR 1 ROW FOR READ ONLY                       01933000
         EXEC  SQL                                                     *01934000
               OPEN SAESQ002                                            01935000
         ICM   15,B'1111',SQLCODE      IS RETURN CODE ZERO ?            01936000
         BNZ   *+8                     NO - RETURN ERROR                01937000
         LR    12,9                    LOAD RETURN ADDRESS              01938000
         BR    12                      RETURN TO FETCH ROUTINE          01939000
         L     14,SQW@RET              LOAD RETURN ADDRESS              01940000
