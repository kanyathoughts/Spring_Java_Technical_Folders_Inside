*                                                                       02390000
         DS    0H                      END OF SQL STATEMENTS            02391000
         DC    XL16'FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF'                   02392000
*                                                                       02393000
*                                                                       02394000
**********************************************************************  02395000
* DUMMY ENTRY POINT DSNHLI                                              02396000
*   MUST NOT BE MODIFIED.                                               02397000
**********************************************************************  02398000
*                                                                       02399000
         ENTRY DSNHLI                                                   02400000
DSNHLI   DS    0H                                                       02401000
         L     15,SQW@CAF              LOAD ENTRY POINT TO ATTACH       02402000
         BR    15                      BRANCH TO ATTACH FACILITY        02403000
*                                                                       02404000
*                                                                       02405000
**********************************************************************  02406000
* CONVERSION ROUTINES INTO AND OUT OF ASSEMBLER HOST VARIABLES          02407000
*   REQUIRED IF THE COBOL RECORD STRUCTURE IS NOT ALIGNED.              02408000
*   MUST NOT BE MODIFIED.                                               02409000
**********************************************************************  02410000
*                                                                       02411000
CONV#RTN CSECT                         CONVERSION ROUTINES SECTION      02412000
CONV#RTN AMODE ANY                                                      02413000
CONV#RTN RMODE ANY                                                      02414000
IN#HOST  DS    0H                      MOVE INTO HOST VARIABLES         02415000
         USING IN#HOST,5               ESTABLISH BASE REGISTER          02416000
         LA    6,CONV#TAB              LOAD CONVERSION TABLE ADDRESS    02417000
IN#HOST1 CLI   0(6),X'FF'              END OF TABLE?                    02418000
         BER   4                         YES - RETURN TO SQL            02419000
         LA    0,COBREC                LOAD RECORD AREA ADDRESS         02420000
         AH    0,0(6)                  ADD OFFSET WITHIN RECORD AREA    02421000
         LA    14,ASMREC               LOAD HOST VARIABLE AREA ADDRESS  02422000
         AH    14,2(6)                 ADD OFFSET WITHIN HOST VARIABLES 02423000
         LH    1,4(6)                  LOAD LENGTH TO MOVE              02424000
         LR    15,1                    SET BOTH LENGTHS EQUAL           02425000
         MVCL  14,0                    MOVE TO HOST VARIABLES           02426000
         LA    6,8(6)                  INCREMENT TO NEXT ENTRY          02427000
         B     IN#HOST1                LOOP UNTIL END OF TABLE          02428000
*                                                                       02429000
IN#KEY   DS    0H                      MOVE KEYS INTO HOST VARIABLES    02430000
         USING IN#KEY,5                ESTABLISH BASE REGISTER          02431000
         LA    6,CONV#TAB              LOAD CONVERSION TABLE ADDRESS    02432000
IN#KEY1  CLI   0(6),X'FF'              END OF TABLE?                    02433000
         BER   4                         YES - RETURN TO SQL            02434000
         SLR   0,0                     CLEAR FOR INSERT                 02435000
         IC    0,6(6)                  INSERT KEY FIELD ENTRY           02436000
         NR    0,3                     FOR THE CURRENT KEY?             02437000
         BZ    IN#KEY2                   NO - GO FOR NEXT ENTRY         02438000
         LA    0,COBREC                LOAD RECORD AREA ADDRESS         02439000
         AH    0,0(6)                  ADD OFFSET WITHIN RECORD AREA    02440000
         LA    14,ASMREC               LOAD HOST VARIABLE AREA ADDRESS  02441000
         AH    14,2(6)                 ADD OFFSET WITHIN HOST VARIABLES 02442000
         LH    1,4(6)                  LOAD LENGTH TO MOVE              02443000
         LR    15,1                    SET BOTH LENGTHS EQUAL           02444000
         MVCL  14,0                    MOVE TO HOST VARIABLES           02445000
         TM    7(6),X'80'              SIGNED DECIMAL FIELD?            02446000
         BNO   IN#KEY2                   NO - GO FOR NEXT ENTRY         02447000
         BCTR  14,0                    DECREMENT TO SIGN BYTE           02448000
         TM    0(14),X'0A'             VALID SIGN BYTE?                 02449000
         BO    IN#KEY2                   YES - GO FOR NEXT ENTRY        02450000
         TM    0(14),X'0C'             VALID SIGN BYTE?                 02451000
         BO    IN#KEY2                   YES - GO FOR NEXT ENTRY        02452000
         OI    0(14),X'0F'               NO  - MAKE IT VALID            02453000
IN#KEY2  LA    6,8(6)                  INCREMENT TO NEXT ENTRY          02454000
         B     IN#KEY1                 LOOP UNTIL END OF TABLE          02455000
*                                                                       02456000
OUT#REC  DS    0H                      MOVE HOST VARIABLES INTO RECORD  02457000
         USING OUT#REC,5               ESTABLISH BASE REGISTER          02458000
         LA    6,CONV#TAB              LOAD CONVERSION TABLE ADDRESS    02459000
OUT#REC1 CLI   0(6),X'FF'              END OF TABLE?                    02460000
         BER   4                         YES - RETURN TO SQL            02461000
         LA    0,COBREC                LOAD RECORD AREA ADDRESS         02462000
         AH    0,0(6)                  ADD OFFSET WITHIN RECORD AREA    02463000
         LA    14,ASMREC               LOAD HOST VARIABLE AREA ADDRESS  02464000
         AH    14,2(6)                 ADD OFFSET WITHIN HOST VARIABLES 02465000
         LH    1,4(6)                  LOAD LENGTH TO MOVE              02466000
         LR    15,1                    SET BOTH LENGTHS EQUAL           02467000
         MVCL  0,14                    MOVE TO RECORD AREA              02468000
         LA    6,8(6)                  INCREMENT TO NEXT ENTRY          02469000
         B     OUT#REC1                LOOP UNTIL END OF TABLE          02470000
*                                                                       02471000
OUT#KEY  DS    0H                      MOVE KEYS INTO RECORD            02472000
         USING OUT#KEY,5               ESTABLISH BASE REGISTER          02473000
         LA    6,CONV#TAB              LOAD CONVERSION TABLE ADDRESS    02474000
OUT#KEY1 CLI   0(6),X'FF'              END OF TABLE?                    02475000
         BER   4                         YES - RETURN TO SQL            02476000
         TM    6(6),X'80'              FOR THE PRIMARY KEY?             02477000
         BZ    OUT#KEY2                  NO - GO FOR NEXT ENTRY         02478000
         LA    0,COBREC                LOAD RECORD AREA ADDRESS         02479000
         AH    0,0(6)                  ADD OFFSET WITHIN RECORD AREA    02480000
         LA    14,ASMREC               LOAD HOST VARIABLE AREA ADDRESS  02481000
         AH    14,2(6)                 ADD OFFSET WITHIN HOST VARIABLES 02482000
