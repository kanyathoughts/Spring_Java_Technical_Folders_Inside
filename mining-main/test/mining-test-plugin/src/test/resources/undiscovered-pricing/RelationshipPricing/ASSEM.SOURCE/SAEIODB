SQL#0032 DS    0H                                                       01565000
         USING SQL#0032,12             ESTABLISH BASE REGISTER          01566000
         LA    3,X'80'                 LOAD MASK FOR KEY 0              01567000
         L     5,=A(IN#KEY)            LOAD KEY CONVERSION ROUTINE      01568000
         BALR  4,5                     MOVE INTO HOST VARIABLES         01569000
         EXEC  SQL                                                     *01570000
               SELECT ZERO_NBR,                                        *01571000
                   AUDIT_DATE_KEY,                                     *01572000
                   AUDIT_TIME_KEY,                                     *01573000
                   UNIQUE_CODE,                                        *01574000
                   AUDIT_TIME_SUBS,                                    *01575000
                   TRAN_CODE,                                          *01576000
                   INST_NBR,                                           *01577000
                   FORMAT_ID,                                          *01578000
                   LOG_CODE,                                           *01579000
                   SEGM_ID                                             *01580000
                 INTO :ZERONBR,                                        *01581000
                   :AUDDATK,                                           *01582000
                   :AUDTIMK,                                           *01583000
                   :UNIQCDE,                                           *01584000
                   :AUDTIMS,                                           *01585000
                   :TRANSCD,                                           *01586000
                   :INSTNBR,                                           *01587000
                   :FORMATID,                                          *01588000
                   :LOGCODE,                                           *01589000
                   :SEGID                                              *01590000
                 FROM SAE                                              *01591000
                 WHERE ZERO_NBR = :ZERONBR AND                         *01592000
                   AUDIT_DATE_KEY = :AUDDATK AND                       *01593000
                   AUDIT_TIME_KEY = :AUDTIMK AND                       *01594000
                   UNIQUE_CODE = :UNIQCDE AND                          *01595000
                   AUDIT_TIME_SUBS = :AUDTIMS AND                      *01596000
                   TRAN_CODE = :TRANSCD AND                            *01597000
                   INST_NBR = :INSTNBR AND                             *01598000
                   FORMAT_ID = :FORMATID AND                           *01599000
                   LOG_CODE = :LOGCODE AND                             *01600000
                   SEGM_ID = :SEGID                                    *01601000
                 FETCH FIRST 1 ROW ONLY                                 01602000
         L     14,SQW@RET              LOAD RETURN ADDRESS              01603000
         BR    14                      RETURN TO CALLER                 01604000
         LTORG                                                          01605000
