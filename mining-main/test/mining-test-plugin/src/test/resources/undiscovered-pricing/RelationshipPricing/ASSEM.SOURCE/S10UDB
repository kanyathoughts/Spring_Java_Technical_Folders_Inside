                   ACCUM_DESC_12                                       *00336000
                 FROM S10                                              *00337000
                 WHERE                                                 *00338000
                   INST_NBR = :INST AND                                *00339000
                   RECORD_NBR = :RECNBR AND                            *00340000
                   TRAN_PROFILE = :TRANPROF                            *00341000
                 FOR UPDATE OF                                         *00342000
                   AUDIT_DATE,                                         *00343000
