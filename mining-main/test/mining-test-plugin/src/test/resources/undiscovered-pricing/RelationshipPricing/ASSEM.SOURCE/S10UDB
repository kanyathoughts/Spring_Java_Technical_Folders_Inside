                   ACCUM_COND_10,                                      *00316000
                   ACCUM_10,                                           *00317000
                   REL_CODE_10,                                        *00318000
                   REQM_OPTION_10,                                     *00319000
                   TRAN_COUNT_10,                                      *00320000
                   TRAN_AMOUNT_10,                                     *00321000
                   ACCUM_DESC_10,                                      *00322000
                   ACCUM_COND_11,                                      *00323000
                   ACCUM_11,                                           *00324000
                   REL_CODE_11,                                        *00325000
                   REQM_OPTION_11,                                     *00326000
                   TRAN_COUNT_11,                                      *00327000
                   TRAN_AMOUNT_11,                                     *00328000
                   ACCUM_DESC_11,                                      *00329000
                   ACCUM_COND_12,                                      *00330000
