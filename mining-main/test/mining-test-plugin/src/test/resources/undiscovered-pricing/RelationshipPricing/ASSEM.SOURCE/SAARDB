         ENTRY DSNHLI                                                   01239000
DSNHLI   DS    0H                                                       01240000
         L     15,SQW@CAF              LOAD ENTRY POINT TO ATTACH       01241000
         BR    15                      BRANCH TO ATTACH FACILITY        01242000
*                                                                       01243000
**********************************************************************  01244000
* CONVERSION TABLE VALUES FOR ALL FIELDS IN THIS TABLE                  01245000
**********************************************************************  01246000
*                                                                       01247000
* CONVTAB1 TABLE ENTRY FORMAT IS:                                       01248000
*        DC    H'RRRR',H'VVVV',H'LLLL',X'KK',X'DD'                      01249000
* OR:                                                                   01250000
*        DC    H'RRRR',H'VVVV',X'ZZPP',X'KK',X'DD'                      01251000
* WHERE:                                                                01252000
*   RRRR = RECORD AREA OFFSET                                           01253000
*   VVVV = HOST VARIABLE AREA OFFSET                                    01254000
*   LLLL = HALFWORD LENGTH TO MOVE                                      01255000
*   ZZPP = CONVERT ZONED/PACKED LENGTHS (MINUS 1)                       01256000
*   KK   = KEY FIELD MASK:                                              01257000
*            80 = KEY 0 FIELD                                           01258000
*            40 = KEY 1 FIELD                                           01259000
*            20 = KEY 2 FIELD                                           01260000
*            10 = KEY 3 FIELD                                           01261000
*   DD   = DATA FIELD MASK:                                             01262000
*            80 = RECORD FIELD IS PACKED                                01263000
*            40 = HOST VARIABLE IS PACKED                               01264000
*            20 = NULLABLE FIELD                                        01265000
*            01 = DATE FIELD                                            01266000
*            02 = TIME FIELD                                            01267000
*                                                                       01268000
CONVTAB1 DS    0H                      RECORD/HOST VARIABLE CONVERSIONS 01269000
         DC    H'0000',H'0000',H'0028',X'80',X'00'                      01270000
         DC    H'0028',H'0028',H'0409',X'00',X'00'                      01271000
         DC    8X'FF'                                                   01272000
*                                                                       01273000
* CONVTAB2 TABLE COLUMNS ENTRY FORMAT IS:                               01274000
*        DC    H'RRRR',H'LLLL',H'TTTT'                                  01275000
* OR:                                                                   01276000
*        DC    H'RRRR',X'PPSS',H'TTTT'                                  01277000
* WHERE:                                                                01278000
*   RRRR = RECORD AREA OFFSET                                           01279000
*   LLLL = HALFWORD LENGTH OF HOST VARIABLE (NON-DECIMAL DATA TYPE)     01280000
*   PPSS = PRECISION AND SCALE (DECIMAL DATA TYPE)                      01281000
*   TTTT = SQLDA DATA TYPE:                                             01282000
*            452 = CHAR       453 = CHAR NULLABLE                       01283000
*            484 = DECIMAL    485 = DECIMAL NULLABLE                    01284000
*            496 = INTEGER    497 = INTEGER NULLABLE                    01285000
*            500 = SMALLINT   501 = SMALLINT NULLABLE                   01286000
*                                                                       01287000
CONVTAB2 DS    0F                      RECORD DATA ATTRIBUTE TABLE      01288000
