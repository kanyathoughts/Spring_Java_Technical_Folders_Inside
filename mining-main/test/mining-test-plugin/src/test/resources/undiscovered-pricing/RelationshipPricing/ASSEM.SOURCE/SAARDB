                 FROM SAA                                              *01001000
                 WHERE                                                 *01002000
                   INST_NBR >                                          *01003000
                     :INST                                             *01004000
                 ORDER BY INST_NBR ASC,                                *01005000
                   PROD_CODE ASC,                                      *01006000
                   ACCT_NBR ASC                                        *01007000
                 OPTIMIZE FOR 1 ROW FOR READ ONLY                       01008000
         EXEC  SQL OPEN SAAGT003                                        01009000
         ICM   15,B'1111',SQLCODE      IS RETURN CODE ZERO ?            01010000
         BNZ   *+14                    NO - RETURN ERROR                01011000
         MVC   SQWCSRCA,=A(CLSGT003)   SET CURSOR CLOSE ROUTINE ADDRESS 01012000
         L     12,SQWCSRRA             LOAD RETURN ADDRESS              01013000
         BR    12                      RETURN TO FETCH ROUTINE          01014000
         L     14,SQW@RET              LOAD RETURN ADDRESS              01015000
         BR    14                      RETURN TO CALLER                 01016000
         LTORG                                                          01017000
