{
  "name": "mining-web-ui-frontend",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build -c production",
    "dev-build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build -c local --watch",
    "start": "ng serve -c local --host 127.0.0.1",
    "lint": "eslint --color -c .eslintrc.js --ext .ts ./src/app && stylelint \"src/**/*.less\" && htmlhint \"src\" --config .htmlhintrc",
    "lint-e2e": "eslint --color -c .eslintrc.js --ext .ts ./e2e/src",
    "test": "npm run env -s && ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-no-coverage": "ng test --watch=false --browsers=ChromeHeadless",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && node --unhandled-rejections=strict node_modules/@angular/cli/bin/ng e2e -c local --webdriver-update=false",
    "e2e-jenkins": "webdriver-manager update --versions.chrome 99.0.4844.0 --ignore_ssl && npm run env -s && node --unhandled-rejections=strict node_modules/@angular/cli/bin/ng e2e -c e2e-jenkins --webdriver-update=false",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "generate": "ng generate",
    "postinstall": "node build-scripts/puppeteer-retrieve-cached-chromium && node build-scripts/puppeteer-cache-chromium",
    "check-license": "node build-scripts/check-license",
    "preinstall": "node build-scripts/unlock-snapshot-dependencies",
    "pre-commit": "npm run lint && npm run test-headless",
    "prepare": "cd ../../../../../ && husky install main/mining-ui/src/main/web/.husky",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/elements": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/localize": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@antv/g2plot": "~2.4.31",
    "@apollo/client": "3.6.9",
    "@innowake/mining-api-angular-client": "^99.9.99-TRUNK-MINING-SNAPSHOT",
    "@ngx-translate/core": "^14.0.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "apollo-angular": "^6.0.0",
    "browserslist": "^4.16.3",
    "caniuse-lite": "^1.0.30001192",
    "canvg": "^4.0.1",
    "core-js": "^3.9.1",
    "graphql": "^16.8.1",
    "html2canvas": "1.3.4",
    "lodash": "^4.17.21",
    "ng-zorro-antd": "^17.3.0",
    "ngx-infinite-scroll": "^17.0.1",
    "rxjs": "^7.5.5",
    "tslib": "^2.0.0",
    "url-polyfill": "^1.1.12",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^6.0.0",
    "yfiles": "../../../lib/yFiles/yfiles-26.0.2.tgz",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.1",
    "@angular-devkit/architect": "^0.1703.0",
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/language-service": "^17.3.0",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/fragment-matcher": "5.0.0",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@ngx-rocket/scripts": "^5.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@yworks/optimizer": "^1.5.0",
    "css-loader": "^5.2.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-sonarjs": "^0.24.0",
    "htmlhint": "^1.1.2",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jspdf": "~2.5.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "~0.0.36",
    "license-checker": "^25.0.1",
    "monaco-editor": "0.33.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^21.5.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-less": "^3.0.1",
    "svg2pdf.js": "2.2.3",
    "ts-node": "~9.1.1",
    "typescript": "~5.4.2"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
