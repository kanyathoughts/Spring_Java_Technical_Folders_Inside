<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
    <mappings>
        <mapping pattern="**/*" type="COBOL/PROGRAM" folder="programs"/>
        <mapping pattern="**/*" type="COBOL/COPYBOOK" folder="copies"/>
        <mapping pattern="**/*" type="COBOL/COPYLIB" folder="copylibs"/>
        <mapping pattern="**/*" type="JCL/JOB" folder="jobs"/>
        <mapping pattern="**/*" type="JCL/PROC" folder="procs"/>
        <mapping pattern="**/*" type="JCL/CONTROLCARD" folder="controlcards"/>
        <mapping pattern="**/*" type="JCL/INCLUDE" folder="include"/>
        <mapping pattern="**/*" type="ASSEMBLER/MACRO" folder="macro"/>
        <mapping pattern="**/*" type="ASSEMBLER/PROGRAM" folder=""/>
        <mapping pattern="**/*" type="XML" folder=""/>
        <mapping pattern="**/*" type="PL1/MAINPROGRAM" folder="programs"/>
        <mapping pattern="**/*" type="PL1/PROGRAM" folder="programs"/>
        <mapping pattern="**/*" type="PL1/COPYBOOK" folder="copies"/>
        <mapping pattern="**/*" type="NATURAL/PROGRAM" folder="programs"/>
        <mapping pattern="**/*" type="NATURAL/SUBPROGRAM" folder="subprograms"/>
        <mapping pattern="**/*" type="NATURAL/SUBROUTINE" folder="subroutines"/>
        <mapping pattern="**/*" type="NATURAL/FUNCTION" folder="functions"/>
        <mapping pattern="**/*" type="NATURAL/COPYCODE" folder="copies"/>
        <mapping pattern="**/*" type="NATURAL/DDM" folder="ddms"/>
        <mapping pattern="**/*" type="NATURAL/HELP" folder="helproutines"/>
        <mapping pattern="**/*" type="NATURAL/LDA" folder="ldas"/>
        <mapping pattern="**/*" type="NATURAL/IW_LDA" folder="ldas"/>
        <mapping pattern="**/*" type="NATURAL/PDA" folder="pdas"/>
        <mapping pattern="**/*" type="NATURAL/IW_PDA" folder="pdas"/>
        <mapping pattern="**/*" type="NATURAL/GDA" folder="gdas"/>
        <mapping pattern="**/*" type="NATURAL/IW_GDA" folder="gdas"/>
        <mapping pattern="**/*" type="NATURAL/MAP" folder="maps"/>
        <mapping pattern="**/*" type="NATURAL/TEXT" folder="texts"/>
        <mapping pattern="**/*" type="NATURAL/ERROR_MESSAGE" folder="messages"/>
        <mapping pattern="**/*" type="NATURAL/CLASS" folder="classes"/>
        <mapping pattern="**/*" type="NATURAL/CPM" folder="cpms"/>
        <mapping pattern="**/*" type="NATURAL/ADAPTER" folder="adapters"/>
        <mapping pattern="**/*" type="NATURAL/ADAPTVIEW" folder="adapters"/>
        <mapping pattern="**/*" type="NATURAL/DIALOG" folder="dialogs"/>
        <mapping pattern="**/*" type="NATURAL/DIALOG_PRIV_RES" folder="dialogs"/>
        <mapping pattern="**/*" type="BINARY" folder=""/>
        <mapping pattern="**/*" type="EASYTRIEVE/PROGRAM" folder=""/>
        <mapping pattern="**/*" type="EASYTRIEVE/MACRO_FILE" folder=""/>
        <mapping pattern="**/*" type="C/PROGRAM" folder=""/>
        <mapping pattern="**/*" type="C/HEADER" folder=""/>
        <mapping pattern="**/*" type="CPP/PROGRAM" folder=""/>
        <mapping pattern="**/*" type="CPP/HEADER" folder=""/>
        <mapping pattern="**/*" type="CSD/LIST" folder=""/>
        <mapping pattern="**/*" type="CSD/EXTRACT" folder=""/>
        <mapping pattern="**/*" type="IMS/DBD" folder="dbd"/>
        <mapping pattern="**/*" type="IMS/HELPTXT" folder="help"/>
        <mapping pattern="**/*" type="IMS/HDAMPARM" folder="dbdh"/>
        <mapping pattern="**/*" type="IMS/PSB" folder="psb"/>
        <mapping pattern="**/*" type="IMS/MFS" folder="maps"/>
        <mapping pattern="**/*" type="DCL" folder="dcl"/>
        <mapping pattern="**/*" type="FMS/FORM" folder="fms"/>
        <mapping pattern="**/*" type="IFDL/FORM" folder="ifdl"/>
        <mapping pattern="**/*" type="CDO/FILE" folder="cdo"/>
        <mapping pattern="**/*" type="SQLMOD" folder="sqlmod"/>
        <mapping pattern="**/*" type="BASIC/OBJECT" folder=""/>
        <mapping pattern="**/*" type="SQL/SCRIPT" folder=""/>
        <mapping pattern="**/*" type="VAX/MACRO" folder="mar"/>
        <mapping pattern="**/*" type="NONE" folder=""/>
        <mapping pattern="**/*" type="JAVA/COMPILATION_UNIT" folder=""/>
        <mapping pattern="**/*" type="ECL" folder=""/>
        <mapping pattern="**/*" type="ECL/JOB" folder="jobs"/>
        <mapping pattern="**/*" type="CICS/BMS_MAPSET" folder="maps"/>
        <mapping pattern="**/*" type="VB/MODULE" folder="module"/>
        <mapping pattern="**/*" type="VB/CLASS" folder="class"/>
        <mapping pattern="**/*" type="VB/FORM" folder="form"/>
        <mapping pattern="**/*" type="VB/CONTROL" folder="control"/>
        <mapping pattern="**/*" type="VB/PROJECT" folder="project"/>
        <mapping pattern="**/*" type="VB/WORKSPACE" folder="workspace"/>
        <mapping pattern="**/*" type="VB/ACTIVEX_DOCUMENT" folder="activexdocument"/>
        <mapping pattern="**/*" type="VB/DESIGNER_FILE" folder="designerfile"/>
        <mapping pattern="**/*" type="WINDOWS/DLL" folder="dll"/>
        <mapping pattern="**/*" type="WINDOWS/OCX" folder="ocx"/>
        <mapping pattern="**/*" type="CSHARP/COMPILATION_UNIT" folder="compilation_unit"/>
        <mapping pattern="**/*" type="CSHARP/PROJECT" folder="project"/>
        <mapping pattern="**/*" type="CSHARP/SOLUTION" folder="solution"/>
    </mappings>
    <properties>
        <property language="ASSEMBLER">
            <setting key="UNKNOWN_TOKEN_THRESHOLD" value="0.2"/>
        </property>
        <property language="BASIC">
            <setting key="RETAIN_FILE_EXTENSION" value="true"/>
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="VB">
            <setting key="ENABLE" value="false"/>
            <setting key="RETAIN_FILE_EXTENSION" value="true"/>
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="BINARY">
            <setting key="PRINTABLE_CHAR_THRESHOLD" value="0.85"/>
        </property>
        <property language="COBOL">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="COPYBOOK_PIC_THRESHOLD" value="0.08"/>
            <setting key="COBOL_PGM_ID_AS_NAME" value="false"/>
            <setting key="COBOL_IMS_DEPENDENCY_METRICS_ENABLE" value="false"/>
        </property>
        <property language="CSD">
            <setting key="PARSER_TIMEOUT" value="2000"/>
            <setting key="CSD_PARSER_DUMPXML" value="true"/>
        </property>
        <property language="EASYTRIEVE">
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="C">
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="CPP">
            <setting key="RETAIN_FILE_EXTENSION" value="true"/>
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="SQL">
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
        <property language="JCL">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="STEP_NAME" value="SYSIN"/>
            <setting key="STEP_NAME" value="SYSTSIN"/>
            <setting key="STEP_NAME" value="INPUT"/>
            <setting key="DD_KEYWORD" value="*"/>
            <setting key="DD_KEYWORD" value="PATH"/>
            <setting key="DD_KEYWORD" value="SUBSYS"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSDUMP"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSABEND"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSMDUMP"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSUDUMP"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSCHK"/>
            <setting key="DD_IGNORE_DEPENDENCY" value="SYSCKEOV"/>
        </property>
        <property language="NATURAL">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="DECIMAL_CHARACTER" value="."/>
            <setting key="INPUT_DELIMITER" value=","/>
            <setting key="LANGUAGE_CODES" value="1"/>
        </property>
        <property language="ORACLE">
            <setting key="CDO_PATTERN_THRESHOLD" value="0.10"/>
            <setting key="SQLMOD_PATTERN_THRESHOLD" value="0.05"/>
        </property>
        <property language="PL1">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="PL1_PARSER_MARGIN_START" value="2"/>
            <setting key="PL1_PARSER_MARGIN_END" value="72"/>
            <setting key="PL1_PARSER_ANS" value="-1"/>
            <setting key="PL1_PARSER_NOT_SYMBOL" value="^"/>
            <setting key="PL1_PARSER_OR_SYMBOL" value="!"/>
        </property>
        <property language="VMS">
            <setting key="FMS_PATTERN_THRESHOLD" value="0.05"/>
            <setting key="DCL_PATTERN_THRESHOLD" value="0.2"/>
        </property>
        <property language="JAVA">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="EXPORT_CHUNK_SIZE" value="10000"/>
            <setting key="INCLUDE_PACKAGES" value=""/>
            <setting key="EXCLUDE_PACKAGES" value=""/>
        </property>
        <property language="ECL">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="ECL_PATTERN_THRESHOLD" value="0.75"/>
        </property>
        <property language="IMS">
            <setting key="PARSER_TIMEOUT" value="30"/>
            <setting key="IMS_DBD_FIELDS_ENABLE" value="false"/>
        </property>
        <property language="CSHARP">
            <setting key="RETAIN_FILE_EXTENSION" value="true"/>
            <setting key="PARSER_TIMEOUT" value="30"/>
        </property>
    </properties>
    <retain_dir_structure>true</retain_dir_structure>
    <exclude_utility>true</exclude_utility>
    <collect_statements>true</collect_statements>
    <undiscovered_folder>undiscovered-entities</undiscovered_folder>
    <output_format>EXCEL</output_format>
    <file_detection_types>
        <file_detection_type type="MAIN" priority="1"/>
        <file_detection_type type="TOKEN" priority="2"/>
        <file_detection_type type="DEPENDENCY" priority="3"/>
        <file_detection_type type="EXTENSION" priority="4"/>
    </file_detection_types>
    <dead_code_metrics>false</dead_code_metrics>
</config>
