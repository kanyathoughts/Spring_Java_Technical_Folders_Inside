 SIMPLE: PROCEDURE OPTIONS(MAIN);
   IF FXD2 > FXD3 THEN PUT SKIP LIST("FXD2 > FXD3");
   ELSE PUT SKIP LIST("FXD2 <= FXD3");
      IF FXD2 <= FXD3 THEN PUT SKIP LIST("FXD2 <= FXD3");
   ELSE PUT SKIP LIST("FXD2 > FXD3"); 
   
   PUT SKIP LIST('START FIXED BINARY');
   PUT SKIP LIST('RESULTS');
   
   IF FXB1 <= FXB2 THEN PUT SKIP LIST("FXB1 <= FXB2");
   ELSE PUT SKIP LIST("FXB1 > FXB2"); 
   IF FXB1 <= FXB3 THEN PUT SKIP LIST("FXB1 <= FXB3");
   ELSE PUT SKIP LIST("FXB1 > FXB3");
    SELECT ;   
    OTHERWISE PUT SKIP LIST('NO MATCH FOUND');
    END;
    IF RESULT
    THEN 
       PUT SKIP LIST("FXD1 >= FXD2", RESULT);
          IF FXD1 <= FXD2 THEN PUT SKIP LIST("FXD1 <= FXD2");
   ELSE PUT SKIP LIST("FXD1 > FXD2"); 
   IF FXD1 <= FXD3 THEN PUT SKIP LIST("FXD1 <= FXD3");
   ELSE PUT SKIP LIST("FXD1 > FXD3"); 
    SELECT ;
    WHEN (LABEL_A()) 
     PUT SKIP LIST('INSIDE WHEN LABEL_A');
    WHEN (LABEL_B())
     PUT SKIP LIST('INSIDE WHEN LABEL_B');       
    END; 
 END SIMPLE;