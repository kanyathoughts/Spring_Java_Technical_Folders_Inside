 PROGRAM: PROCEDURE OPTIONS(MAIN);

   DECLARE DecimalVar DECIMAL(8, 2);
   DECLARE BinaryVar BINARY(4);
   DECLARE BitVar BIT(8);
   DECLARE CharVar CHAR(20);
   
   DecimalVar = 123.45;
   BinaryVar = '1010'B;
   BitVar = BITAND(255, 170);
   CharVar = 'OpenAI Chatbot';
   
   PUT SKIP LIST('DecimalVar: ', DecimalVar);
   PUT SKIP LIST('BinaryVar: ', BinaryVar);
   PUT SKIP LIST('BitVar: ', BitVar);
   PUT SKIP LIST('CharVar: ', CharVar);

   END PROGRAM;
  