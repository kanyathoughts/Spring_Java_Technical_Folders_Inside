QCKSET   DFHMSD TYPE=MAP, X
             STORAGE=AUTO, X
             MODE=OUT, X
             LANG=COBOL, X
             TIOAPFX=YES
QCKMAP1  DFHMDI SIZE=(24,80),LINE=1,COLUMN=1,CTRL=ALARM
SCREEN1  DFHMDF POS = (01,01), X
                LENGTH = 7, X
                INITIAL = ‘SCREEN1’, X
                  ATTRB = (PROT,NORM)
*
QCKMAP2  DFHMDI SIZE=(24,80),LINE=1,COLUMN=1,CTRL=ALARM
SCREEN2  DFHMDF POS = (01,01), X
                LENGTH = 7, X
                INITIAL = ‘SCREEN2’, X
                  ATTRB = (PROT,NORM)
        DFHMSD  TYPE = FINAL
      END
