CREATE MASK INCOME_MASK ON CUSTOMER
    FOR COLUMN INCOME RETURN
             CASE WHEN(INCOME = 0) 
               THEN 100000
               ELSE INCOME
             END
    ENABLE;

COMMIT;

ALTER TABLE CUSTOMER 
        ACTIVATE COLUMN ACCESS CONTROL;

COMMIT;

SELECT DISTINCT B FROM
    (SELECT STATE, MAX(INCOME) FROM CUSTOMER 
        GROUP BY STATE) 
     X(A, B)
       WHERE B > 10000;   