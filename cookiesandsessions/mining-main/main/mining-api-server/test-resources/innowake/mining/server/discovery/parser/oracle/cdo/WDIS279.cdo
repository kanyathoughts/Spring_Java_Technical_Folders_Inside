SET VER
SET DEF BEN_LOC_FLD
SHOW DEF
START_TRANSACTION
!                                                                             
DEFINE FIELD BENI000_RETRY_LIMIT	BASED ON CDD_GEN_FLD.CLIENT_RETRY_LIMIT.
DEFINE FIELD BENI000_CLIENT_USERNAME 	BASED ON CDD_GEN_FLD.CLIENT_VMS_USERNAME.
DEFINE FIELD BENI000_CLIENT_NODENAME 	BASED ON CDD_GEN_FLD.CLIENT_DESKTOP_NODENAME.
DEFINE FIELD BENI000_CLIENT_ROUTINE 	BASED ON CDD_GEN_FLD.CLIENT_ROUTINE.
DEFINE FIELD BENI000_ACTION_CODE 	BASED ON CDD_GEN_FLD.IOM_REQUESTED_ACTION.
DEFINE FIELD BENI000_ACTION_UPDATE
       COMPUTED BY IF BENI000_ACTION_CODE EQ "UPDATE" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_INSERT
       COMPUTED BY IF BENI000_ACTION_CODE EQ "INSERT" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_DELETE
       COMPUTED BY IF BENI000_ACTION_CODE EQ "DELETE" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_COMMIT
       COMPUTED BY IF BENI000_ACTION_CODE EQ "COMMIT" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_OPEN
       COMPUTED BY IF BENI000_ACTION_CODE EQ "OPEN" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_FETCH
       COMPUTED BY IF BENI000_ACTION_CODE EQ "FETCH" THEN 1 ELSE 0.
DEFINE FIELD BENI000_ACTION_CLOSE
       COMPUTED BY IF BENI000_ACTION_CODE EQ "CLOSE" THEN 1 ELSE 0.
!
!
DEFINE RECORD   BEN_WKSP.BENI000_CONTROL_WKSP.
       BENI000_CLIENT_USERNAME.
       BENI000_CLIENT_NODENAME.
       BENI000_CLIENT_ROUTINE.
       BENI000_RETRY_LIMIT.
       BENI000_ACTION_CODE.
       BENI000_ACTION_UPDATE.
       BENI000_ACTION_INSERT.
       BENI000_ACTION_DELETE.
       BENI000_ACTION_COMMIT.
       BENI000_ACTION_OPEN.
       BENI000_ACTION_FETCH.
       BENI000_ACTION_CLOSE.
END BENI000_CONTROL_WKSP RECORD.
!
COMMIT
EXIT
