100  TYPE ' '
     SET COUNTF = '        '
     QUERY COUNT TST01: ENTER COUNT ADJUSTMENT  +/- (7 DIGITS)
     JUMP 110  IF LEN(COUNT) = 0
     JUMP 105  IF COUNT >  9999999
     JUMP 105  IF COUNT < -9999999
     SET COUNTA = FMT(COUNT,0,0)
     JUMP 110  IF COUNTA = '%COUNT%'
     SET COUNTA = FMT(COUNT,0,0' +')
     JUMP 110  IF COUNTA = '%COUNT%'
105  TYPE ' '
     TYPE 'TST01: ERROR - VALID RESPONSE IS +/- (0-9999999)'
     JUMP 100
110  JUMP + 3  IF COUNT < 0
     SET COUNTF = FMT(ABS(COUNT),7,0,' ') ' '
     JUMP 115
     SET COUNTF = FMT(ABS(COUNT),7,0,' ') '-'
115  TYPE ' '
     SET AMOUNTF = '                '
     QUERY AMOUNT TST01: ENTER AMOUNT ADJUSTMENT +/- (PIC S9(13)V99)
     JUMP 125 IF LEN(AMOUNT) = 0
     JUMP 120 IF AMOUNT >  999999999999999
     JUMP 120 IF AMOUNT < -999999999999999
     SET AMOUNTA = FMT(AMOUNT,0,0)
     JUMP 125 IF AMOUNTA = '%AMOUNT%'
     SET AMOUNTA = FMT(AMOUNT,0,0,' +')
     JUMP 125 IF AMOUNTA = '%AMOUNT%'
120  TYPE ' '
     TYPE 'TST01: ERROR - VALID RESPONSE IS +/- (0V00-9999999999999V99)'
     JUMP 115
125  JUMP +03 IF AMOUNT  < 0
     SET AMOUNTF = FMT(ABS(AMOUNT),15,0,' ') ' '
     JUMP 130
     SET AMOUNTF = FMT(ABS(AMOUNT),15,0,' ') '-'
130  TYPE ' '
     SET FORMF = '        '
     QUERY FORM TST01: ENTER FORMS COUNT ADJUSTMENT +/- (7 DIGITS)
     JUMP 140 IF LEN(FORM) = 0
     JUMP 135 IF COUNT >  9999999
     JUMP 135 IF COUNT < -9999999
     SET FORMA = FMT(FORM,0,0)
     JUMP 140 IF FORMA = '%FORM%'
     SET FORMA = FMT(FORM,0,0,' +')
     JUMP 140 IF FORMA = '%FORM%'
135  TYPE ' '
     TYPE 'TST01: ERROR - VALID RESPONSE IS +/- (0-9999999)'
     JUMP 130
140  JUMP + 3  IF FORM  < 0
     SET FORMF = FMT(ABS(FORM),7,0,' ') ' '
     JUMP + 2
     SET FORMF = FMT(ABS(FORM),7,0,' ') '-'
     GEN '%F8849%%MF%%RC%TST-OIL-ADJ%CRN%%COUNTF%%AMOUNTF%%FORMF%'
     JUMP 15
200  GEN @END
     GEN @FIN
     RESEQUENCE
     LIST
     SAVE         *TST01ADJECL.
     REP          *TST01ADJECL.
     TYPE 'TST01: PLEASE @START THE FOLLOWING ECL RUNSTREAM'
     TYPE 'TST01: @START %QUALIFIER%*TST01ADJECL.'
     XCTS
*SAVE TSTADJ
CALL TSTADJ
XCTS

