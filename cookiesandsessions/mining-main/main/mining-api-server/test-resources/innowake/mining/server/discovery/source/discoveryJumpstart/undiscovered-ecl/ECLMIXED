@CTS,IN TPF$.
NUMBER
     QUERY CHECK
   5 QUERY QUALIFIER TST01: ENTER QUALIFIER
     SET QUALSTA = CSF('@QUAL %QUALIFIER%')
     JUMP +5  IF QUALSTA <> 0
     PXQT QUAL %QUALIFIER%
     SET TST0103STA = CSF('@CAT,P *TST0103.')
     JUMP 10 IF TST0103STA <> 0
     TYPE 'TST01: ERROR - "%QUALIFIER%" IMPROPER QUALIFIER'
     JUMP 5
  10 CSF 'CAT,P     *TST01ADJECL(+1).'
     NEW            *TST01ADJECL.
     REL            *TST0105.
     GEN  @RUN       TAI04,BA/TAI04,TAI04
     GEN  @SYM,F     PRINT$,,NTRPR
     GEN  @QUAL %QUALIFIER%
     GEN  @CAT,P    *TST0105.
     GEN  @DATA,ILQ *TST0105.
  15 TYPE ' '
     QUERY RC TST01: ENTER RECORD CODE: B=TMF,I=TTF,C=TESTT-T OR E=EXIT
     BRANCH %RC%, 'B', 'I', 'C', 'E' (20,25,30,200)
     TYPE 'TST01: ERROR - VALID RESPONSE IS B OR I OR C OR E'
     JUMP 15
  20 SET MF = '2'
     SET RC = 'B'
     JUMP 35
  25 SET MF = '1'
     SET RC = 'I'
     JUMP 35
  30 SET MF = '2'
     SET RC = 'C'
     JUMP 35
  35 QUERY F8849 TST01: FORM 8849 CLAIMS? Y OR N
     BRANCH %F8849%, 'Y', 'N' (40,45)
     TYPE 'TST01: ERROR - VALID RESPONSE IS Y OR N'
     JUMP 35
  40 SET F8849 = '*'
     TYPE ' '
     JUMP 50
  45 SET F8849 = ' '
     TYPE ' '
     JUMP 50
