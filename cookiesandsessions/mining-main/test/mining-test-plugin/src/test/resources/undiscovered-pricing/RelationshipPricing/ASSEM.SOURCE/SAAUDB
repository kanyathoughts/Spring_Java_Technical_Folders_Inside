                   RSV_FLAG_2,                                         *00389000
                   RSV_FLAG_3,                                         *00390000
                   RSV_AREA                                            *00391000
                 FETCH FIRST 1 ROW ONLY                                 00392000
         EXEC  SQL OPEN SAAUPD0                                         00393000
         MVC   SQWCSUCA,=A(CLSUC0)     SET CURSOR CLOSE ROUTINE ADDRESS 00394000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00395000
         BR    14                      RETURN TO CALLER                 00396000
         LTORG                                                          00397000
*                                                                       00398000
**********************************************************************  00399000
* FETCH FROM UPDATE CURSOR STATEMENT FOR PRIMARY KEY:                   00400000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00401000
*   IT WILL FOLLOW A SUCCESSFUL SELECT UPDATE STATEMENT TO RETRIEVE     00402000
*     THE ACTUAL ROW.                                                   00403000
**********************************************************************  00404000
*                                                                       00405000
FETUC0   DS    0H                                                       00406000
         USING FETUC0,12               ESTABLISH BASE REGISTER          00407000
         B     *+6                     BRANCH AROUND ADCON              00408000
BASFET0  DC    AL2(4096)                                                00409000
         LR    3,10                    LOAD SECOND BASE FOR SQLDSECT    00410000
         AH    3,BASFET0               ADD 4K                           00411000
         EXEC  SQL FETCH SAAUPD0                                       *00412000
                 INTO                                                  *00413000
                   :AUDDATE,                                           *00414000
                   :AUDTIME,                                           *00415000
                   :AUDUSER,                                           *00416000
                   :AUDORG,                                            *00417000
                   :SCINCENO,                                          *00418000
                   :INCENQ,                                            *00419000
                   :SCMODEL,                                           *00420000
                   :DISCAMT,                                           *00421000
                   :DISCPCT,                                           *00422000
                   :RINCENO,                                           *00423000
                   :RINCENQ,                                           *00424000
                   :RATMODL,                                           *00425000
                   :RATTYP,                                            *00426000
                   :RATOPT,                                            *00427000
                   :RATEPCT,                                           *00428000
                   :PROCOPT,                                           *00429000
                   :RTAMT1,                                            *00430000
                   :RTPCT1,                                            *00431000
                   :RTAMT2,                                            *00432000
                   :RTPCT2,                                            *00433000
                   :RTAMT3,                                            *00434000
                   :RTPCT3,                                            *00435000
                   :RTAMT4,                                            *00436000
                   :RTPCT4,                                            *00437000
                   :RTAMT5,                                            *00438000
                   :RTPCT5,                                            *00439000
                   :RTAMT6,                                            *00440000
                   :RTPCT6,                                            *00441000
                   :RTAMT7,                                            *00442000
                   :RTPCT7,                                            *00443000
                   :RTAMT8,                                            *00444000
