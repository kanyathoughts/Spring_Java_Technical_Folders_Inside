**********************************************************************  00015000
* SQI COMMUNICATION AREA DSECT ADDRESSED BY REGISTER 11                 00016000
**********************************************************************  00017000
*                                                                       00018000
COM#AREA DSECT                         SQI COMMUNICATION AREA           00019000
SQW@CAF  DS    F                       ADDRESS OF CALL ATTACH FACILITY  00020000
SQW@RET  DS    F                       RETURN TO THIS SQI ADDRESS       00021000
SQWSQLCA DS    XL136                   SQLCA FOR I/O MODULE USE         00022000
         ORG   SQWSQLCA                                                 00023000
         EXEC  SQL INCLUDE SQLCA                                        00024000
SQWSQLDL DS    F                       LENGTH OF DYNAMIC SQLDSECT       00025000
SQWSQLD  DS    F                       DYNAMIC SQLDSECT                 00026000
SQW@SQRX DS    F                       SQI INTERFACE ROUTINE ADDRESS    00027000
SQWPARM1 DS    F                       SQI PARAMETER 1 (MODULE ADDRESS) 00028000
SQWINTHV DS    F                       INTEGER HOST VARIABLE            00029000
SQWCSRBR DS    F                       CURSOR ROUTINE BASE REGISTER     00030000
SQWCSRRA DS    F                       CURSOR ROUTINE RETURN ADDRESS    00031000
SQWRSFVC DS    H                       ROWSET FETCH SQLVAR COUNT        00032000
SQWKMRP  DS    CL1                     SQI KEY MASK / ROUTINE POINTER   00033000
         DS    CL5                     RESERVED                         00034000
SQWIOSI  DS    0CL24                   I/O MODULE STATE INFORMATION     00035000
SQWSQLDA DS    F                       ROWSET SQLDA STORAGE ADDRESS     00036000
SQWRSRMX DS    H                       ROWSET MAX NUMBER OF ROWS        00037000
SQWRSRCT DS    H                       ROWSET RETURNED ROW COUNT        00038000
SQWRSROC DS    H                       ROWSET ROW OCCURS COUNT          00039000
SQWRSFCD DS    H                       ROWSET FETCH SQLCODE             00040000
SQWCSRCA DS    F                       READ CURSOR CLOSE ROUTINE ADDR   00041000
SQWCSUCA DS    F                       UPDATE CURSOR CLOSE ROUTINE ADDR 00042000
SQWCSRLV DS    CL1                     CURSOR CASCADE LEVEL LIMIT       00043000
SQWCSRCC DS    CL1                     CURSOR CASCADE COUNT             00044000
SQWCSRSP DS    H                       CURSOR ROUTINE POINTER           00045000
         DS    CL4                     RESERVED                         00046000
SQWCMD   DS    CL2                     DBS COMMAND CODE                 00047000
SQWORG   DS    CL6                     RECORD ORG CODE                  00048000
ASMREC   DS    0F                      ASSEMBLER RECORD (ALIGNED)       00049000
INST1    DS    CL4                                                      00050000
PRDCDE1  DS    CL6                                                      00051000
ACCTNBR1 DS    CL18                                                     00052000
MODEL    DS    CL10                                                     00053000
INST2    DS    CL4                                                      00054000
PRDCDE2  DS    CL6                                                      00055000
ACCTNBR2 DS    CL18                                                     00056000
AUDDATE  DS    PL5'0.'                                                  00057000
AUDTIME  DS    PL5'0.'                                                  00058000
AUDUSER  DS    CL8                                                      00059000
AUDORG   DS    CL6                                                      00060000
ACCTST   DS    CL1                                                      00061000
BRCH     DS    PL3'0.'                                                  00062000
ACCTTYPE DS    PL2'0.'                                                  00063000
SCINCENO DS    CL1                                                      00064000
RINCENO  DS    CL1                                                      00065000
APPLNBR  DS    CL2                                                      00066000
ACCTBAL1 DS    PL8'0.00'                                                00067000
ACCTBAL2 DS    PL8'0.00'                                                00068000
PRIMEBAL DS    PL8'0.00'                                                00069000
ACMBBAL1 DS    PL8'0.00'                                                00070000
ACMBBAL2 DS    PL8'0.00'                                                00071000
ACMBBAL3 DS    PL8'0.00'                                                00072000
ACMBBAL4 DS    PL8'0.00'                                                00073000
CASHBAL  DS    PL8'0.00'                                                00074000
ACCTRATE DS    PL8'0.00'                                                00075000
SCBAL    DS    PL8'0.00'                                                00076000
SECSVBAL DS    PL8'0.00'                                                00077000
DAMTFLAG DS    CL1                                                      00078000
DPCTFLAG DS    CL1                                                      00079000
RPCTFLAG DS    CL1                                                      00080000
DAMTOVRD DS    PL6'0.00'                                                00081000
DPCTOVRD DS    PL3'0.00000'                                             00082000
RPCTOVRD DS    PL3'0.00000'                                             00083000
RAREA    DS    CL43                                                     00084000
*                                                                       00085000
         ORG   ASMREC+(2000-L'SQWADATA) POINT TO ADDITIONAL DATA        00086000
SQWADATA DS    0CL400                  ADDITIONAL DATA PASSED TO MODULE 00087000
SQWSEGF  DS    CL102                   SEGMENTED FROM KEY VALUE         00088000
SQWSEGT  DS    CL102                   SEGMENTED TO KEY VALUE           00089000
SQWAUDIT DS    CL99                    CALLERS AUDIT DATA               00090000
         DS    CL97                    RESERVED                         00091000
*                                                                       00092000
INDVARS  DS    0H                      NULL INDICATOR VARIABLES         00093000
INDVARX  DS    0H                                                       00094000
INDVARL  EQU   INDVARX-INDVARS         NULL INDICATOR AREA LENGTH       00095000
*                                                                       00096000
*                                                                       00097000
**********************************************************************  00098000
* ROWSET SQLDA AREA ADDRESSED BY REGISTER 2                             00099000
**********************************************************************  00100000
*                                                                       00101000
SQDSQLDA DSECT                         ROWSET SQLDA AREA                00102000
*                                                                       00103000
**********************************************************************  00104000
* PROGRAM TABLE HEADER SECTION:                                         00105000
*   THIS SECTION CONTAINS STATIC DESCRIPTIVE FIELDS.                    00106000
**********************************************************************  00107000
*                                                                       00108000
SAHRDB   CSECT                         PROGRAM TABLE SECTION            00109000
SAHRDB   AMODE ANY                                                      00110000
SAHRDB   RMODE ANY                                                      00111000
         DC    CL8'SAHRDB  '           PROGRAM ID                       00112000
         DC    CL1' '                                                   00113000
         DC    CL8'&SYSDATE'           ASSEMBLY DATE                    00114000
         DC    CL1' '                                                   00115000
         DC    CL5'&SYSTIME'           ASSEMBLY TIME                    00116000
         DC    CL1' '                                                   00117000
         DC    A(SQLDLEN)              SQLDSECT SIZE                    00118000
         DC    5A(0)                   RESERVED                         00119000
         DC    AL2(0)                  RESERVED                         00120000
         DC    AL2(INDVARL)            NULL INDICATOR AREA LENGTH       00121000
         DC    A(CONVTAB1)             RECORD/HOST CONVERSION TABLE     00122000
         DC    A(CONVTAB2)             SQLDA DATA TYPE/LENGTH TABLE     00123000
         DC    A(STM#TAB)              STATEMENT TABLE ADDRESS          00124000
         DC    CL43'COPYRIGHT 2017 INFOR. ALL RIGHTS RESERVED. '        00125000
         DC    CL29'WWW.INFOR.COM                '                      00125001
*                                                                       00126000
**********************************************************************  00127000
* STATEMENT TABLE SECTION:                                              00128000
*   THIS SECTION DEFINES AN ENTRY VECTOR FOR EACH SQL STATEMENT.        00129000
**********************************************************************  00130000
*                                                                       00131000
STM#TAB  CSECT                         STATEMENT TABLE SECTION          00132000
STM#TAB  AMODE ANY                                                      00133000
STM#TAB  RMODE ANY                                                      00134000
         DC    A(SELIN0)               SELECT INTO (KEY 0)              00135000
         DC    A(SELIN1)               SELECT INTO (KEY 1)              00136000
         DC    A(SELIN2)               SELECT INTO (KEY 2)              00137000
         DC    A(SELIN3)               SELECT INTO (KEY 3)              00138000
         DC    12A(0)                  UDB MODULE VECTORS               00139000
         DC    A(SELXC0)               SELECT SEQ CURSOR (KEY 0)        00140000
         DC    A(SELXC1)               SELECT SEQ CURSOR (KEY 1)        00141000
         DC    A(SELXC2)               SELECT SEQ CURSOR (KEY 2)        00142000
         DC    A(SELXC3)               SELECT SEQ CURSOR (KEY 3)        00143000
         DC    A(FETDC0)               FETCH SEQ DATA CURSOR (KEY 0)    00144000
         DC    A(FETDC1)               FETCH SEQ DATA CURSOR (KEY 1)    00145000
         DC    A(FETDC2)               FETCH SEQ DATA CURSOR (KEY 2)    00146000
         DC    A(FETDC3)               FETCH SEQ DATA CURSOR (KEY 3)    00147000
         DC    A(FETKC0)               FETCH SEQ KEY CURSOR (KEY 0)     00148000
         DC    A(FETKC1)               FETCH SEQ KEY CURSOR (KEY 1)     00149000
         DC    A(FETKC2)               FETCH SEQ KEY CURSOR (KEY 2)     00150000
         DC    A(FETKC3)               FETCH SEQ KEY CURSOR (KEY 3)     00151000
         DC    A(CLSXC0)               CLOSE SEQ CURSOR (KEY 0)         00152000
         DC    A(CLSXC1)               CLOSE SEQ CURSOR (KEY 1)         00153000
         DC    A(CLSXC2)               CLOSE SEQ CURSOR (KEY 2)         00154000
         DC    A(CLSXC3)               CLOSE SEQ CURSOR (KEY 3)         00155000
         DC    A(SELKY0)               SELECT KEY (KEY 0)               00156000
         DC    A(SELKY1)               SELECT KEY (KEY 1)               00157000
         DC    A(SELKY2)               SELECT KEY (KEY 2)               00158000
         DC    A(SELKY3)               SELECT KEY (KEY 3)               00159000
         DC    10A(0)                  UDB MODULE VECTORS               00160000
         DC    4X'FF'                                                   00161000
*                                                                       00162000
**********************************************************************  00163000
* SQL STATEMENT SECTION:                                                00164000
*   THIS SECTION CONTAINS ALL THE STATIC SQL STATEMENTS REQUIRED        00165000
*     TO SUPPORT THIS TABLE.                                            00166000
*   THE INDICATED STATEMENTS MAY BE MODIFIED, AS LONG AS THE RESULTS    00167000
*     ARE EQUIVALENT.                                                   00168000
**********************************************************************  00169000
*                                                                       00170000
SQL#STMT CSECT                         SQL STATEMENT SECTION            00171000
SQL#STMT AMODE ANY                                                      00172000
SQL#STMT RMODE ANY                                                      00173000
         USING SQDSQLDA,2              ADDRESS ROWSET SQLDA AREA        00174000
         USING SQLDSECT,10             ADDRESS SQLDSECT                 00175000
         USING COM#AREA,11             ADDRESS COMMAREA                 00176000
*                                                                       00177000
**********************************************************************  00178000
* SELECT INTO STATEMENT BY PRIMARY KEY:                                 00179000
*   THIS STATEMENT SUPPORTS THE GET (WITHOUT LOCK) VERB.                00180000
**********************************************************************  00181000
*                                                                       00182000
SELIN0   DS    0H                                                       00183000
         USING SELIN0,12               ESTABLISH BASE REGISTER          00184000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00185000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00186000
         BALR  14,15                   MOVE REQUESTED DATA              00187000
         EXEC  SQL SELECT                                              *00188000
                   AUDIT_DATE,                                         *00189000
                   AUDIT_TIME,                                         *00190000
                   AUDIT_USER,                                         *00191000
                   AUDIT_ORG,                                          *00192000
                   ACCT_STATUS,                                        *00193000
                   BRANCH,                                             *00194000
                   ACCT_TYPE,                                          *00195000
                   SVC_INCEN_OPT,                                      *00196000
                   RATE_INCEN_OPT,                                     *00197000
                   APPL_NBR,                                           *00198000
                   ACCT_BAL_1,                                         *00199000
                   ACCT_BAL_2,                                         *00200000
                   PRIMARY_BAL,                                        *00201000
                   ACCT_CMB_BAL_1,                                     *00202000
                   ACCT_CMB_BAL_2,                                     *00203000
                   ACCT_CMB_BAL_3,                                     *00204000
                   ACCT_CMB_BAL_4,                                     *00205000
                   CASH_BALANCE,                                       *00206000
                   ACCT_RATE_BAL,                                      *00207000
                   SVC_CHG_BAL,                                        *00208000
                   SEC_SVC_BAL,                                        *00209000
                   DISC_AMT_FLAG,                                      *00210000
                   DISC_PCT_FLAG,                                      *00211000
                   RATE_PCT_FLAG,                                      *00212000
                   DISC_AMT_OVRD,                                      *00213000
                   DISC_PCT_OVRD,                                      *00214000
                   RATE_PCT_OVRD,                                      *00215000
                   RSV_AREA                                            *00216000
                 INTO                                                  *00217000
                   :AUDDATE,                                           *00218000
                   :AUDTIME,                                           *00219000
                   :AUDUSER,                                           *00220000
                   :AUDORG,                                            *00221000
                   :ACCTST,                                            *00222000
                   :BRCH,                                              *00223000
                   :ACCTTYPE,                                          *00224000
                   :SCINCENO,                                          *00225000
                   :RINCENO,                                           *00226000
                   :APPLNBR,                                           *00227000
                   :ACCTBAL1,                                          *00228000
                   :ACCTBAL2,                                          *00229000
                   :PRIMEBAL,                                          *00230000
                   :ACMBBAL1,                                          *00231000
                   :ACMBBAL2,                                          *00232000
                   :ACMBBAL3,                                          *00233000
                   :ACMBBAL4,                                          *00234000
                   :CASHBAL,                                           *00235000
                   :ACCTRATE,                                          *00236000
                   :SCBAL,                                             *00237000
                   :SECSVBAL,                                          *00238000
                   :DAMTFLAG,                                          *00239000
                   :DPCTFLAG,                                          *00240000
                   :RPCTFLAG,                                          *00241000
                   :DAMTOVRD,                                          *00242000
                   :DPCTOVRD,                                          *00243000
                   :RPCTOVRD,                                          *00244000
                   :RAREA                                              *00245000
                 FROM SAH                                              *00246000
                 WHERE                                                 *00247000
                   ACCT_INST_1 = :INST1 AND                            *00248000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00249000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00250000
                   MODEL = :MODEL AND                                  *00251000
                   ACCT_INST_2 = :INST2 AND                            *00252000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00253000
                   ACCT_NBR_2 = :ACCTNBR2                              *00254000
                 FETCH FIRST 1 ROW ONLY                                 00255000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00256000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00257000
         BALR  14,15                   MOVE REQUESTED DATA              00258000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00259000
         BR    14                      RETURN TO CALLER                 00260000
         LTORG                                                          00261000
*                                                                       00262000
**********************************************************************  00263000
* SELECT COUNT STATEMENT BY PRIMARY KEY:                                00264000
*   THIS STATEMENT SUPPORTS THE LOCATE VERB.                            00265000
**********************************************************************  00266000
*                                                                       00267000
SELKY0   DS    0H                                                       00268000
         USING SELKY0,12               ESTABLISH BASE REGISTER          00269000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00270000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00271000
         BALR  14,15                   MOVE REQUESTED DATA              00272000
         EXEC  SQL SELECT                                              *00273000
                   COUNT(*)                                            *00274000
                 INTO                                                  *00275000
                   :SQWINTHV                                           *00276000
                 FROM SAH                                              *00277000
                 WHERE                                                 *00278000
                   ACCT_INST_1 = :INST1 AND                            *00279000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00280000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00281000
                   MODEL = :MODEL AND                                  *00282000
                   ACCT_INST_2 = :INST2 AND                            *00283000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00284000
                   ACCT_NBR_2 = :ACCTNBR2                              *00285000
                 FETCH FIRST 1 ROW ONLY                                 00286000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00287000
         CLC   SQLCODE,=F'0'           GOOD RETURN FROM SQL?            00288000
         BNER  14                      NO - RETURN TO CALLER            00289000
         CLC   SQWINTHV,=F'0'          ANY ROWS MATCH WHERE CLAUSE?     00290000
         BNER  14                      YES - RETURN ZERO SQLCODE        00291000
         MVC   SQLCODE,=F'+100'        SET SQLCODE TO ROW NOT FOUND     00292000
         BR    14                      RETURN TO CALLER                 00293000
         LTORG                                                          00294000
*                                                                       00295000
**********************************************************************  00296000
* SELECT SEQUENTIAL AND OPEN SEQUENTIAL CURSOR STATEMENTS BY PRIMARY    00297000
* KEY:                                                                  00298000
*   THESE STATEMENTS SUPPORT THE GET-GE, GET-GE-LOCK, GET-NEXT,         00299000
*     AND GET-NEXT-LOCK VERBS.                                          00300000
*   A SELECT UPDATE STATEMENT WILL FOLLOW IN THE CASE OF A              00301000
*     GET-GE-LOCK OR GET-NEXT-LOCK VERB.                                00302000
**********************************************************************  00303000
*                                                                       00304000
SELXC0   DS    0H                                                       00305000
         USING SELXC0,12               ESTABLISH BASE REGISTER          00306000
         MVI   SQWKMRP,X'86'           SET HOST KEY 0 & CURSOR POINTER  00307000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00308000
         BALR  14,15                   MOVE DATA & SET CURSOR POINTER   00309000
         LA    12,SELXC0P              LOAD VECTOR TABLE ADDRESS        00310000
         AH    12,SQWCSRSP             COMPUTE POINTER TO OPEN ROUTINE  00311000
         L     12,0(12)                LOAD OPEN ROUTINE ADDRESS        00312000
         BR    12                      GO TO CURSOR OPEN ROUTINE        00313000
SELXC0P  DC    A(SELGE001)                                              00314000
         DC    (KY0COLMS-01)A(0)                                        00315000
         DC    A(SELGE002)                                              00316000
         DC    (KY0COLMS-02)A(0)                                        00317000
         DC    A(SELGE003)                                              00318000
         DC    (KY0COLMS-03)A(0)                                        00319000
         DC    A(SELGE004)                                              00320000
         DC    (KY0COLMS-04)A(0)                                        00321000
         DC    A(SELGE005)                                              00322000
         DC    (KY0COLMS-05)A(0)                                        00323000
         DC    A(SELGE006)                                              00324000
         DC    (KY0COLMS-06)A(0)                                        00325000
         DC    A(SELGE007)                                              00326000
         LTORG                                                          00327000
*                                                                       00328000
SELGE001 DS    0H                                                       00329000
         USING SELGE001,12             ESTABLISH BASE REGISTER          00330000
         EXEC  SQL DECLARE SAHGE001 CURSOR                             *00331000
               WITH ROWSET POSITIONING                                 *00332000
               FOR SELECT                                              *00333000
                   ACCT_INST_1,                                        *00334000
                   PROD_CODE_1,                                        *00335000
                   ACCT_NBR_1,                                         *00336000
                   MODEL,                                              *00337000
                   ACCT_INST_2,                                        *00338000
                   PROD_CODE_2,                                        *00339000
                   ACCT_NBR_2,                                         *00340000
                   AUDIT_DATE,                                         *00341000
                   AUDIT_TIME,                                         *00342000
                   AUDIT_USER,                                         *00343000
                   AUDIT_ORG,                                          *00344000
                   ACCT_STATUS,                                        *00345000
                   BRANCH,                                             *00346000
                   ACCT_TYPE,                                          *00347000
                   SVC_INCEN_OPT,                                      *00348000
                   RATE_INCEN_OPT,                                     *00349000
                   APPL_NBR,                                           *00350000
                   ACCT_BAL_1,                                         *00351000
                   ACCT_BAL_2,                                         *00352000
                   PRIMARY_BAL,                                        *00353000
                   ACCT_CMB_BAL_1,                                     *00354000
                   ACCT_CMB_BAL_2,                                     *00355000
                   ACCT_CMB_BAL_3,                                     *00356000
                   ACCT_CMB_BAL_4,                                     *00357000
                   CASH_BALANCE,                                       *00358000
                   ACCT_RATE_BAL,                                      *00359000
                   SVC_CHG_BAL,                                        *00360000
                   SEC_SVC_BAL,                                        *00361000
                   DISC_AMT_FLAG,                                      *00362000
                   DISC_PCT_FLAG,                                      *00363000
                   RATE_PCT_FLAG,                                      *00364000
                   DISC_AMT_OVRD,                                      *00365000
                   DISC_PCT_OVRD,                                      *00366000
                   RATE_PCT_OVRD,                                      *00367000
                   RSV_AREA                                            *00368000
                 FROM SAH                                              *00369000
                 WHERE                                                 *00370000
                   ACCT_INST_1 = :INST1 AND                            *00371000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00372000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00373000
                   MODEL = :MODEL AND                                  *00374000
                   ACCT_INST_2 = :INST2 AND                            *00375000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00376000
                   ACCT_NBR_2 >=                                       *00377000
                     :ACCTNBR2                                         *00378000
                 ORDER BY ACCT_NBR_2 ASC                               *00379000
                 OPTIMIZE FOR 1 ROW FOR READ ONLY                       00380000
         EXEC  SQL OPEN SAHGE001                                        00381000
         MVC   SQWCSRCA,=A(CLSGE001)   SET CURSOR CLOSE ROUTINE ADDRESS 00382000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00383000
         BR    14                      RETURN TO CALLER                 00384000
         LTORG                                                          00385000
*                                                                       00386000
SELGE002 DS    0H                                                       00387000
         USING SELGE002,12             ESTABLISH BASE REGISTER          00388000
         EXEC  SQL DECLARE SAHGE002 CURSOR                             *00389000
               WITH ROWSET POSITIONING                                 *00390000
               FOR SELECT                                              *00391000
                   ACCT_INST_1,                                        *00392000
                   PROD_CODE_1,                                        *00393000
                   ACCT_NBR_1,                                         *00394000
                   MODEL,                                              *00395000
                   ACCT_INST_2,                                        *00396000
                   PROD_CODE_2,                                        *00397000
                   ACCT_NBR_2,                                         *00398000
                   AUDIT_DATE,                                         *00399000
                   AUDIT_TIME,                                         *00400000
                   AUDIT_USER,                                         *00401000
                   AUDIT_ORG,                                          *00402000
                   ACCT_STATUS,                                        *00403000
                   BRANCH,                                             *00404000
                   ACCT_TYPE,                                          *00405000
                   SVC_INCEN_OPT,                                      *00406000
                   RATE_INCEN_OPT,                                     *00407000
                   APPL_NBR,                                           *00408000
                   ACCT_BAL_1,                                         *00409000
                   ACCT_BAL_2,                                         *00410000
                   PRIMARY_BAL,                                        *00411000
                   ACCT_CMB_BAL_1,                                     *00412000
                   ACCT_CMB_BAL_2,                                     *00413000
                   ACCT_CMB_BAL_3,                                     *00414000
                   ACCT_CMB_BAL_4,                                     *00415000
                   CASH_BALANCE,                                       *00416000
                   ACCT_RATE_BAL,                                      *00417000
                   SVC_CHG_BAL,                                        *00418000
                   SEC_SVC_BAL,                                        *00419000
                   DISC_AMT_FLAG,                                      *00420000
                   DISC_PCT_FLAG,                                      *00421000
                   RATE_PCT_FLAG,                                      *00422000
                   DISC_AMT_OVRD,                                      *00423000
                   DISC_PCT_OVRD,                                      *00424000
                   RATE_PCT_OVRD,                                      *00425000
                   RSV_AREA                                            *00426000
                 FROM SAH                                              *00427000
                 WHERE                                                 *00428000
                   ACCT_INST_1 = :INST1 AND                            *00429000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00430000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00431000
                   MODEL = :MODEL AND                                  *00432000
                   ACCT_INST_2 = :INST2 AND                            *00433000
