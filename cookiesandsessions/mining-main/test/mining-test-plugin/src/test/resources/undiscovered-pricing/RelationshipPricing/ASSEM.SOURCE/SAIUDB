**********************************************************************  00015000
* SQI COMMUNICATION AREA DSECT ADDRESSED BY REGISTER 11                 00016000
**********************************************************************  00017000
*                                                                       00018000
COM#AREA DSECT                         SQI COMMUNICATION AREA           00019000
SQW@CAF  DS    F                       ADDRESS OF CALL ATTACH FACILITY  00020000
SQW@RET  DS    F                       RETURN TO THIS SQI ADDRESS       00021000
SQWSQLCA DS    XL136                   SQLCA FOR I/O MODULE USE         00022000
         ORG   SQWSQLCA                                                 00023000
         EXEC  SQL INCLUDE SQLCA                                        00024000
SQWSQLDL DS    F                       LENGTH OF DYNAMIC SQLDSECT       00025000
SQWSQLD  DS    F                       DYNAMIC SQLDSECT                 00026000
SQW@SQRX DS    F                       SQI INTERFACE ROUTINE ADDRESS    00027000
SQWPARM1 DS    F                       SQI PARAMETER 1 (MODULE ADDRESS) 00028000
SQWINTHV DS    F                       INTEGER HOST VARIABLE            00029000
SQWCSRBR DS    F                       CURSOR ROUTINE BASE REGISTER     00030000
SQWCSRRA DS    F                       CURSOR ROUTINE RETURN ADDRESS    00031000
SQWRSFVC DS    H                       ROWSET FETCH SQLVAR COUNT        00032000
SQWKMRP  DS    CL1                     SQI KEY MASK / ROUTINE POINTER   00033000
         DS    CL5                     RESERVED                         00034000
SQWIOSI  DS    0CL24                   I/O MODULE STATE INFORMATION     00035000
SQWSQLDA DS    F                       ROWSET SQLDA STORAGE ADDRESS     00036000
SQWRSRMX DS    H                       ROWSET MAX NUMBER OF ROWS        00037000
SQWRSRCT DS    H                       ROWSET RETURNED ROW COUNT        00038000
SQWRSROC DS    H                       ROWSET ROW OCCURS COUNT          00039000
SQWRSFCD DS    H                       ROWSET FETCH SQLCODE             00040000
SQWCSRCA DS    F                       READ CURSOR CLOSE ROUTINE ADDR   00041000
SQWCSUCA DS    F                       UPDATE CURSOR CLOSE ROUTINE ADDR 00042000
SQWCSRLV DS    CL1                     CURSOR CASCADE LEVEL LIMIT       00043000
SQWCSRCC DS    CL1                     CURSOR CASCADE COUNT             00044000
SQWCSRSP DS    H                       CURSOR ROUTINE POINTER           00045000
         DS    CL4                     RESERVED                         00046000
SQWCMD   DS    CL2                     DBS COMMAND CODE                 00047000
SQWORG   DS    CL6                     RECORD ORG CODE                  00048000
ASMREC   DS    0F                      ASSEMBLER RECORD (ALIGNED)       00049000
INST1    DS    CL4                                                      00050000
PRDCDE1  DS    CL6                                                      00051000
ACCTNBR1 DS    CL18                                                     00052000
INST2    DS    CL4                                                      00053000
PRDCDE2  DS    CL6                                                      00054000
ACCTNBR2 DS    CL18                                                     00055000
MODEL2   DS    CL10                                                     00056000
AUDDATE  DS    PL5'0.'                                                  00057000
AUDTIME  DS    PL5'0.'                                                  00058000
AUDUSER  DS    CL8                                                      00059000
AUDORG   DS    CL6                                                      00060000
ALTINST  DS    CL4                                                      00061000
ALTACCT  DS    CL18                                                     00062000
ALTPC    DS    CL6                                                      00063000
RAREA    DS    CL25                                                     00064000
*                                                                       00065000
         ORG   ASMREC+(2000-L'SQWADATA) POINT TO ADDITIONAL DATA        00066000
SQWADATA DS    0CL400                  ADDITIONAL DATA PASSED TO MODULE 00067000
SQWSEGF  DS    CL102                   SEGMENTED FROM KEY VALUE         00068000
SQWSEGT  DS    CL102                   SEGMENTED TO KEY VALUE           00069000
SQWAUDIT DS    CL99                    CALLERS AUDIT DATA               00070000
         DS    CL97                    RESERVED                         00071000
*                                                                       00072000
INDVARS  DS    0H                      NULL INDICATOR VARIABLES         00073000
INDVARX  DS    0H                                                       00074000
INDVARL  EQU   INDVARX-INDVARS         NULL INDICATOR AREA LENGTH       00075000
*                                                                       00076000
*                                                                       00077000
**********************************************************************  00078000
* PROGRAM TABLE HEADER SECTION:                                         00079000
*   THIS SECTION CONTAINS STATIC DESCRIPTIVE FIELDS.                    00080000
**********************************************************************  00081000
*                                                                       00082000
SAIUDB   CSECT                         PROGRAM TABLE SECTION            00083000
SAIUDB   AMODE ANY                                                      00084000
SAIUDB   RMODE ANY                                                      00085000
         DC    CL8'SAIUDB  '           PROGRAM ID                       00086000
         DC    CL1' '                                                   00087000
         DC    CL8'&SYSDATE'           ASSEMBLY DATE                    00088000
         DC    CL1' '                                                   00089000
         DC    CL5'&SYSTIME'           ASSEMBLY TIME                    00090000
         DC    CL1' '                                                   00091000
         DC    A(SQLDLEN)              SQLDSECT SIZE                    00092000
         DC    5A(0)                   RESERVED                         00093000
         DC    AL2(0)                  RESERVED                         00094000
         DC    AL2(INDVARL)            NULL INDICATOR AREA LENGTH       00095000
         DC    A(CONVTAB1)             RECORD/HOST CONVERSION TABLE     00096000
         DC    A(0)                    SQLDA DATA TYPE/LENGTH TABLE     00097000
         DC    A(STM#TAB)              STATEMENT TABLE ADDRESS          00098000
         DC    CL43'COPYRIGHT 2017 INFOR. ALL RIGHTS RESERVED. '        00099000
         DC    CL29'WWW.INFOR.COM                '                      00099001
*                                                                       00100000
**********************************************************************  00101000
* STATEMENT TABLE SECTION:                                              00102000
*   THIS SECTION DEFINES AN ENTRY VECTOR FOR EACH SQL STATEMENT.        00103000
**********************************************************************  00104000
*                                                                       00105000
STM#TAB  CSECT                         STATEMENT TABLE SECTION          00106000
STM#TAB  AMODE ANY                                                      00107000
STM#TAB  RMODE ANY                                                      00108000
         DC    4A(0)                   RDB MODULE VECTORS               00109000
         DC    A(SELUC0)               SELECT UPDATE CURSOR (KEY 0)     00110000
         DC    A(SELUC1)               SELECT UPDATE CURSOR (KEY 1)     00111000
         DC    A(SELUC2)               SELECT UPDATE CURSOR (KEY 2)     00112000
         DC    A(SELUC3)               SELECT UPDATE CURSOR (KEY 3)     00113000
         DC    A(FETUC0)               FETCH UPDATE CURSOR (KEY 0)      00114000
         DC    A(FETUC1)               FETCH UPDATE CURSOR (KEY 1)      00115000
         DC    A(FETUC2)               FETCH UPDATE CURSOR (KEY 2)      00116000
         DC    A(FETUC3)               FETCH UPDATE CURSOR (KEY 3)      00117000
         DC    A(CLSUC0)               CLOSE UPDATE CURSOR (KEY 0)      00118000
         DC    A(CLSUC1)               CLOSE UPDATE CURSOR (KEY 1)      00119000
         DC    A(CLSUC2)               CLOSE UPDATE CURSOR (KEY 2)      00120000
         DC    A(CLSUC3)               CLOSE UPDATE CURSOR (KEY 3)      00121000
         DC    20A(0)                  RDB MODULE VECTORS               00122000
         DC    A(INSROW)               INSERT STATEMENT                 00123000
         DC    A(UPDUC0)               UPDATE STATEMENT (KEY 0)         00124000
         DC    A(UPDUC1)               UPDATE STATEMENT (KEY 1)         00125000
         DC    A(UPDUC2)               UPDATE STATEMENT (KEY 2)         00126000
         DC    A(UPDUC3)               UPDATE STATEMENT (KEY 3)         00127000
         DC    A(DELUC0)               DELETE STATEMENT (KEY 0)         00128000
         DC    A(DELUC1)               DELETE STATEMENT (KEY 1)         00129000
         DC    A(DELUC2)               DELETE STATEMENT (KEY 2)         00130000
         DC    A(DELUC3)               DELETE STATEMENT (KEY 3)         00131000
         DC    A(DELTBL)               DELETE ALL STATEMENT             00132000
         DC    4X'FF'                                                   00133000
*                                                                       00134000
**********************************************************************  00135000
* SQL STATEMENT SECTION:                                                00136000
*   THIS SECTION CONTAINS ALL THE STATIC SQL STATEMENTS REQUIRED        00137000
*     TO SUPPORT THIS TABLE.                                            00138000
*   THE INDICATED STATEMENTS MAY BE MODIFIED, AS LONG AS THE RESULTS    00139000
*     ARE EQUIVALENT.                                                   00140000
**********************************************************************  00141000
*                                                                       00142000
SQL#STMT CSECT                         SQL STATEMENT SECTION            00143000
SQL#STMT AMODE ANY                                                      00144000
SQL#STMT RMODE ANY                                                      00145000
         USING SQLDSECT,10             ADDRESS SQLDSECT                 00146000
         USING COM#AREA,11             ADDRESS COMMAREA                 00147000
*                                                                       00148000
**********************************************************************  00149000
* SELECT UPDATE AND OPEN UPDATE CURSOR STATEMENTS BY PRIMARY KEY:       00150000
*   THESE STATEMENTS SUPPORT THE GET-LOCK VERB.                         00151000
*   THEY ARE ALSO USED AFTER A SUCCESSFUL SELECT SEQUENTIAL STATEMENT   00152000
*     FOR THE GET-GE-LOCK AND GET-NEXT-LOCK VERBS.                      00153000
**********************************************************************  00154000
*                                                                       00155000
SELUC0   DS    0H                                                       00156000
         USING SELUC0,12               ESTABLISH BASE REGISTER          00157000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00158000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00159000
         BALR  14,15                   MOVE REQUESTED DATA              00160000
         EXEC  SQL DECLARE SAIUPD0 CURSOR                              *00161000
               FOR SELECT                                              *00162000
                   ALT_INST,                                           *00163000
                   ALT_ACCT,                                           *00164000
                   ALT_PROD,                                           *00165000
                   AUDIT_DATE,                                         *00166000
                   AUDIT_TIME,                                         *00167000
                   AUDIT_USER,                                         *00168000
                   AUDIT_ORG,                                          *00169000
                   RSV_AREA                                            *00170000
                 FROM SAI                                              *00171000
                 WHERE                                                 *00172000
                   ACCT_INST_1 = :INST1 AND                            *00173000
                   PROD_CODE_1 = :PRDCDE1 AND                          *00174000
                   ACCT_NBR_1 = :ACCTNBR1 AND                          *00175000
                   ACCT_INST_2 = :INST2 AND                            *00176000
                   PROD_CODE_2 = :PRDCDE2 AND                          *00177000
                   ACCT_NBR_2 = :ACCTNBR2 AND                          *00178000
                   MODEL_2 = :MODEL2                                   *00179000
                 FOR UPDATE OF                                         *00180000
                   ALT_INST,                                           *00181000
                   ALT_ACCT,                                           *00182000
                   ALT_PROD,                                           *00183000
                   AUDIT_DATE,                                         *00184000
                   AUDIT_TIME,                                         *00185000
                   AUDIT_USER,                                         *00186000
                   AUDIT_ORG,                                          *00187000
                   RSV_AREA                                            *00188000
                 FETCH FIRST 1 ROW ONLY                                 00189000
         EXEC  SQL OPEN SAIUPD0                                         00190000
         MVC   SQWCSUCA,=A(CLSUC0)     SET CURSOR CLOSE ROUTINE ADDRESS 00191000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00192000
         BR    14                      RETURN TO CALLER                 00193000
         LTORG                                                          00194000
*                                                                       00195000
**********************************************************************  00196000
* FETCH FROM UPDATE CURSOR STATEMENT FOR PRIMARY KEY:                   00197000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00198000
*   IT WILL FOLLOW A SUCCESSFUL SELECT UPDATE STATEMENT TO RETRIEVE     00199000
*     THE ACTUAL ROW.                                                   00200000
**********************************************************************  00201000
*                                                                       00202000
FETUC0   DS    0H                                                       00203000
         USING FETUC0,12               ESTABLISH BASE REGISTER          00204000
         EXEC  SQL FETCH SAIUPD0                                       *00205000
                 INTO                                                  *00206000
                   :ALTINST,                                           *00207000
                   :ALTACCT,                                           *00208000
                   :ALTPC,                                             *00209000
                   :AUDDATE,                                           *00210000
                   :AUDTIME,                                           *00211000
                   :AUDUSER,                                           *00212000
                   :AUDORG,                                            *00213000
                   :RAREA                                               00214000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00215000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00216000
         BALR  14,15                   MOVE REQUESTED DATA              00217000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00218000
         BR    14                      RETURN TO CALLER                 00219000
         LTORG                                                          00220000
*                                                                       00221000
**********************************************************************  00222000
* INSERT STATEMENT:                                                     00223000
*   THIS STATEMENT SUPPORTS THE PUT VERB.                               00224000
**********************************************************************  00225000
*                                                                       00226000
INSROW   DS    0H                                                       00227000
         USING INSROW,12               ESTABLISH BASE REGISTER          00228000
         MVI   SQWKMRP,X'01'           MOVE RECORD TO HOST VARIABLES    00229000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00230000
         BALR  14,15                   MOVE REQUESTED DATA              00231000
         EXEC  SQL INSERT INTO SAI                                     *00232000
                   (                                                   *00233000
                   ACCT_INST_1,                                        *00234000
                   PROD_CODE_1,                                        *00235000
                   ACCT_NBR_1,                                         *00236000
                   ACCT_INST_2,                                        *00237000
                   PROD_CODE_2,                                        *00238000
                   ACCT_NBR_2,                                         *00239000
                   MODEL_2,                                            *00240000
                   AUDIT_DATE,                                         *00241000
                   AUDIT_TIME,                                         *00242000
                   AUDIT_USER,                                         *00243000
                   AUDIT_ORG,                                          *00244000
                   ALT_INST,                                           *00245000
                   ALT_ACCT,                                           *00246000
                   ALT_PROD,                                           *00247000
                   RSV_AREA                                            *00248000
                   )                                                   *00249000
                  VALUES                                               *00250000
                   (                                                   *00251000
                   :INST1,                                             *00252000
                   :PRDCDE1,                                           *00253000
                   :ACCTNBR1,                                          *00254000
                   :INST2,                                             *00255000
                   :PRDCDE2,                                           *00256000
                   :ACCTNBR2,                                          *00257000
                   :MODEL2,                                            *00258000
                   :AUDDATE,                                           *00259000
                   :AUDTIME,                                           *00260000
                   :AUDUSER,                                           *00261000
                   :AUDORG,                                            *00262000
                   :ALTINST,                                           *00263000
                   :ALTACCT,                                           *00264000
                   :ALTPC,                                             *00265000
                   :RAREA                                              *00266000
                   )                                                    00267000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00268000
         BR    14                      RETURN TO CALLER                 00269000
         LTORG                                                          00270000
*                                                                       00271000
**********************************************************************  00272000
* UPDATE STATEMENT BY PRIMARY KEY:                                      00273000
*   THIS STATEMENT SUPPORTS THE REPUT VERB.                             00274000
**********************************************************************  00275000
*                                                                       00276000
UPDUC0   DS    0H                                                       00277000
         USING UPDUC0,12               ESTABLISH BASE REGISTER          00278000
         MVI   SQWKMRP,X'01'           MOVE RECORD TO HOST VARIABLES    00279000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00280000
         BALR  14,15                   MOVE REQUESTED DATA              00281000
         EXEC  SQL UPDATE SAI                                          *00282000
                   SET                                                 *00283000
                     ALT_INST = :ALTINST,                              *00284000
                     ALT_ACCT = :ALTACCT,                              *00285000
                     ALT_PROD = :ALTPC,                                *00286000
                     AUDIT_DATE = :AUDDATE,                            *00287000
                     AUDIT_TIME = :AUDTIME,                            *00288000
                     AUDIT_USER = :AUDUSER,                            *00289000
                     AUDIT_ORG = :AUDORG,                              *00290000
                     RSV_AREA = :RAREA                                 *00291000
                 WHERE CURRENT OF SAIUPD0                               00292000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00293000
         BR    14                      RETURN TO CALLER                 00294000
         LTORG                                                          00295000
*                                                                       00296000
**********************************************************************  00297000
* DELETE STATEMENT BY PRIMARY KEY:                                      00298000
*   THIS STATEMENT SUPPORTS THE DELETE VERB.                            00299000
**********************************************************************  00300000
*                                                                       00301000
DELUC0   DS    0H                                                       00302000
         USING DELUC0,12               ESTABLISH BASE REGISTER          00303000
         EXEC  SQL DELETE FROM SAI                                     *00304000
                 WHERE CURRENT OF SAIUPD0                               00305000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00306000
         BR    14                      RETURN TO CALLER                 00307000
         LTORG                                                          00308000
*                                                                       00309000
**********************************************************************  00310000
* DELETE ALL STATEMENT:                                                 00311000
*   THIS STATEMENT SUPPORTS THE DELETE-FILE VERB.                       00312000
**********************************************************************  00313000
*                                                                       00314000
DELTBL   DS    0H                                                       00315000
         USING DELTBL,12               ESTABLISH BASE REGISTER          00316000
         EXEC  SQL DELETE FROM SAI                                      00317000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00318000
         BR    14                      RETURN TO CALLER                 00319000
         LTORG                                                          00320000
*                                                                       00321000
**********************************************************************  00322000
* CLOSE UPDATE CURSOR STATEMENT FOR PRIMARY KEY:                        00323000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00324000
*   IT WILL FOLLOW THE LAST FETCH FROM UPDATE CURSOR STATEMENT          00325000
*     TO CLOSE THE UPDATE CURSOR.                                       00326000
**********************************************************************  00327000
*                                                                       00328000
CLSUC0   DS    0H                                                       00329000
         USING CLSUC0,12               ESTABLISH BASE REGISTER          00330000
         EXEC  SQL CLOSE SAIUPD0                                        00331000
         XC    SQWCSUCA,SQWCSUCA       CLEAR CURSOR CLOSE ROUTINE ADDR  00332000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00333000
         BR    14                      RETURN TO CALLER                 00334000
         LTORG                                                          00335000
*                                                                       00336000
**********************************************************************  00337000
* SELECT UPDATE AND OPEN UPDATE CURSOR STATEMENTS BY ALTERNATE KEY 1:   00338000
*   THESE STATEMENTS SUPPORT THE GET-LOCK VERB.                         00339000
**********************************************************************  00340000
*                                                                       00341000
SELUC1   DS    0H                                                       00342000
         USING SELUC1,12               ESTABLISH BASE REGISTER          00343000
         MVI   SQWKMRP,X'40'           MOVE RECORD TO HOST FOR KEY 1    00344000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00345000
         BALR  14,15                   MOVE REQUESTED DATA              00346000
         EXEC  SQL DECLARE SAIUPD1 CURSOR                              *00347000
               FOR SELECT                                              *00348000
                   ACCT_INST_1,                                        *00349000
                   PROD_CODE_1,                                        *00350000
                   ACCT_NBR_1,                                         *00351000
                   ACCT_INST_2,                                        *00352000
                   PROD_CODE_2,                                        *00353000
                   ACCT_NBR_2,                                         *00354000
                   MODEL_2,                                            *00355000
                   AUDIT_DATE,                                         *00356000
                   AUDIT_TIME,                                         *00357000
                   AUDIT_USER,                                         *00358000
                   AUDIT_ORG,                                          *00359000
                   RSV_AREA                                            *00360000
                 FROM SAI                                              *00361000
                 WHERE                                                 *00362000
                   ALT_INST = :ALTINST AND                             *00363000
                   ALT_ACCT = :ALTACCT AND                             *00364000
                   ALT_PROD = :ALTPC                                   *00365000
                 FOR UPDATE OF                                         *00366000
                   AUDIT_DATE,                                         *00367000
                   AUDIT_TIME,                                         *00368000
                   AUDIT_USER,                                         *00369000
                   AUDIT_ORG,                                          *00370000
                   RSV_AREA                                            *00371000
                 FETCH FIRST 1 ROW ONLY                                 00372000
         EXEC  SQL OPEN SAIUPD1                                         00373000
         MVC   SQWCSUCA,=A(CLSUC1)     SET CURSOR CLOSE ROUTINE ADDRESS 00374000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00375000
         BR    14                      RETURN TO CALLER                 00376000
         LTORG                                                          00377000
*                                                                       00378000
**********************************************************************  00379000
* FETCH FROM UPDATE CURSOR STATEMENT FOR ALTERNATE KEY 1:               00380000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00381000
*   IT WILL FOLLOW A SUCCESSFUL SELECT UPDATE STATEMENT TO RETRIEVE     00382000
*     THE ACTUAL ROW.                                                   00383000
**********************************************************************  00384000
*                                                                       00385000
FETUC1   DS    0H                                                       00386000
         USING FETUC1,12               ESTABLISH BASE REGISTER          00387000
         EXEC  SQL FETCH SAIUPD1                                       *00388000
                 INTO                                                  *00389000
                   :INST1,                                             *00390000
                   :PRDCDE1,                                           *00391000
                   :ACCTNBR1,                                          *00392000
                   :INST2,                                             *00393000
                   :PRDCDE2,                                           *00394000
                   :ACCTNBR2,                                          *00395000
                   :MODEL2,                                            *00396000
                   :AUDDATE,                                           *00397000
                   :AUDTIME,                                           *00398000
                   :AUDUSER,                                           *00399000
                   :AUDORG,                                            *00400000
                   :RAREA                                               00401000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00402000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00403000
         BALR  14,15                   MOVE REQUESTED DATA              00404000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00405000
         BR    14                      RETURN TO CALLER                 00406000
         LTORG                                                          00407000
*                                                                       00408000
**********************************************************************  00409000
* UPDATE STATEMENT BY ALTERNATE KEY 1:                                  00410000
*   THIS STATEMENT SUPPORTS THE REPUT VERB.                             00411000
**********************************************************************  00412000
