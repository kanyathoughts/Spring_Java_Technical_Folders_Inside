**********************************************************************  00015000
* SQI COMMUNICATION AREA DSECT ADDRESSED BY REGISTER 11                 00016000
**********************************************************************  00017000
*                                                                       00018000
COM#AREA DSECT                         SQI COMMUNICATION AREA           00019000
SQW@CAF  DS    F                       ADDRESS OF CALL ATTACH FACILITY  00020000
SQW@RET  DS    F                       RETURN TO THIS SQI ADDRESS       00021000
SQWSQLCA DS    XL136                   SQLCA FOR I/O MODULE USE         00022000
         ORG   SQWSQLCA                                                 00023000
         EXEC  SQL INCLUDE SQLCA                                        00024000
SQWSQLDL DS    F                       LENGTH OF DYNAMIC SQLDSECT       00025000
SQWSQLD  DS    F                       DYNAMIC SQLDSECT                 00026000
SQW@SQRX DS    F                       SQI INTERFACE ROUTINE ADDRESS    00027000
SQWPARM1 DS    F                       SQI PARAMETER 1 (MODULE ADDRESS) 00028000
SQWINTHV DS    F                       INTEGER HOST VARIABLE            00029000
SQWCSRBR DS    F                       CURSOR ROUTINE BASE REGISTER     00030000
SQWCSRRA DS    F                       CURSOR ROUTINE RETURN ADDRESS    00031000
SQWRSFVC DS    H                       ROWSET FETCH SQLVAR COUNT        00032000
SQWKMRP  DS    CL1                     SQI KEY MASK / ROUTINE POINTER   00033000
         DS    CL5                     RESERVED                         00034000
SQWIOSI  DS    0CL24                   I/O MODULE STATE INFORMATION     00035000
SQWSQLDA DS    F                       ROWSET SQLDA STORAGE ADDRESS     00036000
SQWRSRMX DS    H                       ROWSET MAX NUMBER OF ROWS        00037000
SQWRSRCT DS    H                       ROWSET RETURNED ROW COUNT        00038000
SQWRSROC DS    H                       ROWSET ROW OCCURS COUNT          00039000
SQWRSFCD DS    H                       ROWSET FETCH SQLCODE             00040000
SQWCSRCA DS    F                       READ CURSOR CLOSE ROUTINE ADDR   00041000
SQWCSUCA DS    F                       UPDATE CURSOR CLOSE ROUTINE ADDR 00042000
SQWCSRLV DS    CL1                     CURSOR CASCADE LEVEL LIMIT       00043000
SQWCSRCC DS    CL1                     CURSOR CASCADE COUNT             00044000
SQWCSRSP DS    H                       CURSOR ROUTINE POINTER           00045000
         DS    CL4                     RESERVED                         00046000
SQWCMD   DS    CL2                     DBS COMMAND CODE                 00047000
SQWORG   DS    CL6                     RECORD ORG CODE                  00048000
ASMREC   DS    0F                      ASSEMBLER RECORD (ALIGNED)       00049000
INST     DS    CL4                                                      00050000
RECNBR   DS    CL4                                                      00051000
SS_TABLE DS    CL6                                                      00052000
AUDDATE  DS    PL5'0.'                                                  00053000
AUDTIME  DS    PL5'0.'                                                  00054000
AUDUSER  DS    CL8                                                      00055000
AUDORG   DS    CL6                                                      00056000
BALOPT   DS    CL1                                                      00057000
AMOUNT1  DS    PL6'0.00'                                                00058000
SECSERV1 DS    CL6                                                      00059000
AMOUNT2  DS    PL6'0.00'                                                00060000
SECSERV2 DS    CL6                                                      00061000
AMOUNT3  DS    PL6'0.00'                                                00062000
SECSERV3 DS    CL6                                                      00063000
AMOUNT4  DS    PL6'0.00'                                                00064000
SECSERV4 DS    CL6                                                      00065000
AMOUNT5  DS    PL6'0.00'                                                00066000
SECSERV5 DS    CL6                                                      00067000
AMOUNT6  DS    PL6'0.00'                                                00068000
SECSERV6 DS    CL6                                                      00069000
AMOUNT7  DS    PL6'0.00'                                                00070000
SECSERV7 DS    CL6                                                      00071000
AMOUNT8  DS    PL6'0.00'                                                00072000
SECSERV8 DS    CL6                                                      00073000
AMOUNT9  DS    PL6'0.00'                                                00074000
SECSERV9 DS    CL6                                                      00075000
AMOUNT10 DS    PL6'0.00'                                                00076000
SECSER10 DS    CL6                                                      00077000
*                                                                       00078000
         ORG   ASMREC+(2000-L'SQWADATA) POINT TO ADDITIONAL DATA        00079000
SQWADATA DS    0CL400                  ADDITIONAL DATA PASSED TO MODULE 00080000
SQWSEGF  DS    CL102                   SEGMENTED FROM KEY VALUE         00081000
SQWSEGT  DS    CL102                   SEGMENTED TO KEY VALUE           00082000
SQWAUDIT DS    CL99                    CALLERS AUDIT DATA               00083000
         DS    CL97                    RESERVED                         00084000
*                                                                       00085000
INDVARS  DS    0H                      NULL INDICATOR VARIABLES         00086000
INDVARX  DS    0H                                                       00087000
INDVARL  EQU   INDVARX-INDVARS         NULL INDICATOR AREA LENGTH       00088000
*                                                                       00089000
*                                                                       00090000
**********************************************************************  00091000
* PROGRAM TABLE HEADER SECTION:                                         00092000
*   THIS SECTION CONTAINS STATIC DESCRIPTIVE FIELDS.                    00093000
**********************************************************************  00094000
*                                                                       00095000
S12UDB   CSECT                         PROGRAM TABLE SECTION            00096000
S12UDB   AMODE ANY                                                      00097000
S12UDB   RMODE ANY                                                      00098000
         DC    CL8'S12UDB  '           PROGRAM ID                       00099000
         DC    CL1' '                                                   00100000
         DC    CL8'&SYSDATE'           ASSEMBLY DATE                    00101000
         DC    CL1' '                                                   00102000
         DC    CL5'&SYSTIME'           ASSEMBLY TIME                    00103000
         DC    CL1' '                                                   00104000
         DC    A(SQLDLEN)              SQLDSECT SIZE                    00105000
         DC    5A(0)                   RESERVED                         00106000
         DC    AL2(0)                  RESERVED                         00107000
         DC    AL2(INDVARL)            NULL INDICATOR AREA LENGTH       00108000
         DC    A(CONVTAB1)             RECORD/HOST CONVERSION TABLE     00109000
         DC    A(0)                    SQLDA DATA TYPE/LENGTH TABLE     00110000
         DC    A(STM#TAB)              STATEMENT TABLE ADDRESS          00111000
         DC    CL43'COPYRIGHT 2017 INFOR. ALL RIGHTS RESERVED. '        00112000
         DC    CL29'WWW.INFOR.COM                '                      00112001
*                                                                       00113000
**********************************************************************  00114000
* STATEMENT TABLE SECTION:                                              00115000
*   THIS SECTION DEFINES AN ENTRY VECTOR FOR EACH SQL STATEMENT.        00116000
**********************************************************************  00117000
*                                                                       00118000
STM#TAB  CSECT                         STATEMENT TABLE SECTION          00119000
STM#TAB  AMODE ANY                                                      00120000
STM#TAB  RMODE ANY                                                      00121000
         DC    4A(0)                   RDB MODULE VECTORS               00122000
         DC    A(SELUC0)               SELECT UPDATE CURSOR (KEY 0)     00123000
         DC    A(SELUC1)               SELECT UPDATE CURSOR (KEY 1)     00124000
         DC    A(SELUC2)               SELECT UPDATE CURSOR (KEY 2)     00125000
         DC    A(SELUC3)               SELECT UPDATE CURSOR (KEY 3)     00126000
         DC    A(FETUC0)               FETCH UPDATE CURSOR (KEY 0)      00127000
         DC    A(FETUC1)               FETCH UPDATE CURSOR (KEY 1)      00128000
         DC    A(FETUC2)               FETCH UPDATE CURSOR (KEY 2)      00129000
         DC    A(FETUC3)               FETCH UPDATE CURSOR (KEY 3)      00130000
         DC    A(CLSUC0)               CLOSE UPDATE CURSOR (KEY 0)      00131000
         DC    A(CLSUC1)               CLOSE UPDATE CURSOR (KEY 1)      00132000
         DC    A(CLSUC2)               CLOSE UPDATE CURSOR (KEY 2)      00133000
         DC    A(CLSUC3)               CLOSE UPDATE CURSOR (KEY 3)      00134000
         DC    20A(0)                  RDB MODULE VECTORS               00135000
         DC    A(INSROW)               INSERT STATEMENT                 00136000
         DC    A(UPDUC0)               UPDATE STATEMENT (KEY 0)         00137000
         DC    A(UPDUC1)               UPDATE STATEMENT (KEY 1)         00138000
         DC    A(UPDUC2)               UPDATE STATEMENT (KEY 2)         00139000
         DC    A(UPDUC3)               UPDATE STATEMENT (KEY 3)         00140000
         DC    A(DELUC0)               DELETE STATEMENT (KEY 0)         00141000
         DC    A(DELUC1)               DELETE STATEMENT (KEY 1)         00142000
         DC    A(DELUC2)               DELETE STATEMENT (KEY 2)         00143000
         DC    A(DELUC3)               DELETE STATEMENT (KEY 3)         00144000
         DC    A(DELTBL)               DELETE ALL STATEMENT             00145000
         DC    4X'FF'                                                   00146000
*                                                                       00147000
**********************************************************************  00148000
* SQL STATEMENT SECTION:                                                00149000
*   THIS SECTION CONTAINS ALL THE STATIC SQL STATEMENTS REQUIRED        00150000
*     TO SUPPORT THIS TABLE.                                            00151000
*   THE INDICATED STATEMENTS MAY BE MODIFIED, AS LONG AS THE RESULTS    00152000
*     ARE EQUIVALENT.                                                   00153000
**********************************************************************  00154000
*                                                                       00155000
SQL#STMT CSECT                         SQL STATEMENT SECTION            00156000
SQL#STMT AMODE ANY                                                      00157000
SQL#STMT RMODE ANY                                                      00158000
         USING SQLDSECT,10             ADDRESS SQLDSECT                 00159000
         USING COM#AREA,11             ADDRESS COMMAREA                 00160000
*                                                                       00161000
**********************************************************************  00162000
* SELECT UPDATE AND OPEN UPDATE CURSOR STATEMENTS BY PRIMARY KEY:       00163000
*   THESE STATEMENTS SUPPORT THE GET-LOCK VERB.                         00164000
*   THEY ARE ALSO USED AFTER A SUCCESSFUL SELECT SEQUENTIAL STATEMENT   00165000
*     FOR THE GET-GE-LOCK AND GET-NEXT-LOCK VERBS.                      00166000
**********************************************************************  00167000
*                                                                       00168000
SELUC0   DS    0H                                                       00169000
         USING SELUC0,12               ESTABLISH BASE REGISTER          00170000
         MVI   SQWKMRP,X'80'           MOVE RECORD TO HOST FOR KEY 0    00171000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00172000
         BALR  14,15                   MOVE REQUESTED DATA              00173000
         EXEC  SQL DECLARE S12UPD0 CURSOR                              *00174000
               FOR SELECT                                              *00175000
                   AUDIT_DATE,                                         *00176000
                   AUDIT_TIME,                                         *00177000
                   AUDIT_USER,                                         *00178000
                   AUDIT_ORG,                                          *00179000
                   BAL_OPT,                                            *00180000
                   AMOUNT_01,                                          *00181000
                   SEC_SVC_01,                                         *00182000
                   AMOUNT_02,                                          *00183000
                   SEC_SVC_02,                                         *00184000
                   AMOUNT_03,                                          *00185000
                   SEC_SVC_03,                                         *00186000
                   AMOUNT_04,                                          *00187000
                   SEC_SVC_04,                                         *00188000
                   AMOUNT_05,                                          *00189000
                   SEC_SVC_05,                                         *00190000
                   AMOUNT_06,                                          *00191000
                   SEC_SVC_06,                                         *00192000
                   AMOUNT_07,                                          *00193000
                   SEC_SVC_07,                                         *00194000
                   AMOUNT_08,                                          *00195000
                   SEC_SVC_08,                                         *00196000
                   AMOUNT_09,                                          *00197000
                   SEC_SVC_09,                                         *00198000
                   AMOUNT_10,                                          *00199000
                   SEC_SVC10                                           *00200000
                 FROM S12                                              *00201000
                 WHERE                                                 *00202000
                   INST_NBR = :INST AND                                *00203000
                   RECORD_NBR = :RECNBR AND                            *00204000
                   SEC_SVC_TABLE = :SS_TABLE                           *00205000
                 FOR UPDATE OF                                         *00206000
                   AUDIT_DATE,                                         *00207000
                   AUDIT_TIME,                                         *00208000
                   AUDIT_USER,                                         *00209000
                   AUDIT_ORG,                                          *00210000
                   BAL_OPT,                                            *00211000
                   AMOUNT_01,                                          *00212000
                   SEC_SVC_01,                                         *00213000
                   AMOUNT_02,                                          *00214000
                   SEC_SVC_02,                                         *00215000
                   AMOUNT_03,                                          *00216000
                   SEC_SVC_03,                                         *00217000
                   AMOUNT_04,                                          *00218000
                   SEC_SVC_04,                                         *00219000
                   AMOUNT_05,                                          *00220000
                   SEC_SVC_05,                                         *00221000
                   AMOUNT_06,                                          *00222000
                   SEC_SVC_06,                                         *00223000
                   AMOUNT_07,                                          *00224000
                   SEC_SVC_07,                                         *00225000
                   AMOUNT_08,                                          *00226000
                   SEC_SVC_08,                                         *00227000
                   AMOUNT_09,                                          *00228000
                   SEC_SVC_09,                                         *00229000
                   AMOUNT_10,                                          *00230000
                   SEC_SVC10                                           *00231000
                 FETCH FIRST 1 ROW ONLY                                 00232000
         EXEC  SQL OPEN S12UPD0                                         00233000
         MVC   SQWCSUCA,=A(CLSUC0)     SET CURSOR CLOSE ROUTINE ADDRESS 00234000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00235000
         BR    14                      RETURN TO CALLER                 00236000
         LTORG                                                          00237000
*                                                                       00238000
**********************************************************************  00239000
* FETCH FROM UPDATE CURSOR STATEMENT FOR PRIMARY KEY:                   00240000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00241000
*   IT WILL FOLLOW A SUCCESSFUL SELECT UPDATE STATEMENT TO RETRIEVE     00242000
*     THE ACTUAL ROW.                                                   00243000
**********************************************************************  00244000
*                                                                       00245000
FETUC0   DS    0H                                                       00246000
         USING FETUC0,12               ESTABLISH BASE REGISTER          00247000
         EXEC  SQL FETCH S12UPD0                                       *00248000
                 INTO                                                  *00249000
                   :AUDDATE,                                           *00250000
                   :AUDTIME,                                           *00251000
                   :AUDUSER,                                           *00252000
                   :AUDORG,                                            *00253000
                   :BALOPT,                                            *00254000
                   :AMOUNT1,                                           *00255000
                   :SECSERV1,                                          *00256000
                   :AMOUNT2,                                           *00257000
                   :SECSERV2,                                          *00258000
                   :AMOUNT3,                                           *00259000
                   :SECSERV3,                                          *00260000
                   :AMOUNT4,                                           *00261000
                   :SECSERV4,                                          *00262000
                   :AMOUNT5,                                           *00263000
                   :SECSERV5,                                          *00264000
                   :AMOUNT6,                                           *00265000
                   :SECSERV6,                                          *00266000
                   :AMOUNT7,                                           *00267000
                   :SECSERV7,                                          *00268000
                   :AMOUNT8,                                           *00269000
                   :SECSERV8,                                          *00270000
                   :AMOUNT9,                                           *00271000
                   :SECSERV9,                                          *00272000
                   :AMOUNT10,                                          *00273000
                   :SECSER10                                            00274000
         MVI   SQWKMRP,X'03'           MOVE HOST VARIABLES TO RECORD    00275000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00276000
         BALR  14,15                   MOVE REQUESTED DATA              00277000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00278000
         BR    14                      RETURN TO CALLER                 00279000
         LTORG                                                          00280000
*                                                                       00281000
**********************************************************************  00282000
* INSERT STATEMENT:                                                     00283000
*   THIS STATEMENT SUPPORTS THE PUT VERB.                               00284000
**********************************************************************  00285000
*                                                                       00286000
INSROW   DS    0H                                                       00287000
         USING INSROW,12               ESTABLISH BASE REGISTER          00288000
         MVI   SQWKMRP,X'01'           MOVE RECORD TO HOST VARIABLES    00289000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00290000
         BALR  14,15                   MOVE REQUESTED DATA              00291000
         EXEC  SQL INSERT INTO S12                                     *00292000
                   (                                                   *00293000
                   INST_NBR,                                           *00294000
                   RECORD_NBR,                                         *00295000
                   SEC_SVC_TABLE,                                      *00296000
                   AUDIT_DATE,                                         *00297000
                   AUDIT_TIME,                                         *00298000
                   AUDIT_USER,                                         *00299000
                   AUDIT_ORG,                                          *00300000
                   BAL_OPT,                                            *00301000
                   AMOUNT_01,                                          *00302000
                   SEC_SVC_01,                                         *00303000
                   AMOUNT_02,                                          *00304000
                   SEC_SVC_02,                                         *00305000
                   AMOUNT_03,                                          *00306000
                   SEC_SVC_03,                                         *00307000
                   AMOUNT_04,                                          *00308000
                   SEC_SVC_04,                                         *00309000
                   AMOUNT_05,                                          *00310000
                   SEC_SVC_05,                                         *00311000
                   AMOUNT_06,                                          *00312000
                   SEC_SVC_06,                                         *00313000
                   AMOUNT_07,                                          *00314000
                   SEC_SVC_07,                                         *00315000
                   AMOUNT_08,                                          *00316000
                   SEC_SVC_08,                                         *00317000
                   AMOUNT_09,                                          *00318000
                   SEC_SVC_09,                                         *00319000
                   AMOUNT_10,                                          *00320000
                   SEC_SVC10                                           *00321000
                   )                                                   *00322000
                  VALUES                                               *00323000
                   (                                                   *00324000
                   :INST,                                              *00325000
                   :RECNBR,                                            *00326000
                   :SS_TABLE,                                          *00327000
                   :AUDDATE,                                           *00328000
                   :AUDTIME,                                           *00329000
                   :AUDUSER,                                           *00330000
                   :AUDORG,                                            *00331000
                   :BALOPT,                                            *00332000
                   :AMOUNT1,                                           *00333000
                   :SECSERV1,                                          *00334000
                   :AMOUNT2,                                           *00335000
                   :SECSERV2,                                          *00336000
                   :AMOUNT3,                                           *00337000
                   :SECSERV3,                                          *00338000
                   :AMOUNT4,                                           *00339000
                   :SECSERV4,                                          *00340000
                   :AMOUNT5,                                           *00341000
                   :SECSERV5,                                          *00342000
                   :AMOUNT6,                                           *00343000
                   :SECSERV6,                                          *00344000
                   :AMOUNT7,                                           *00345000
                   :SECSERV7,                                          *00346000
                   :AMOUNT8,                                           *00347000
                   :SECSERV8,                                          *00348000
                   :AMOUNT9,                                           *00349000
                   :SECSERV9,                                          *00350000
                   :AMOUNT10,                                          *00351000
                   :SECSER10                                           *00352000
                   )                                                    00353000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00354000
         BR    14                      RETURN TO CALLER                 00355000
         LTORG                                                          00356000
*                                                                       00357000
**********************************************************************  00358000
* UPDATE STATEMENT BY PRIMARY KEY:                                      00359000
*   THIS STATEMENT SUPPORTS THE REPUT VERB.                             00360000
**********************************************************************  00361000
*                                                                       00362000
UPDUC0   DS    0H                                                       00363000
         USING UPDUC0,12               ESTABLISH BASE REGISTER          00364000
         MVI   SQWKMRP,X'01'           MOVE RECORD TO HOST VARIABLES    00365000
         L     15,SQW@SQRX             LOAD INTERFACE ROUTINE ADDRESS   00366000
         BALR  14,15                   MOVE REQUESTED DATA              00367000
         EXEC  SQL UPDATE S12                                          *00368000
                   SET                                                 *00369000
                     AUDIT_DATE = :AUDDATE,                            *00370000
                     AUDIT_TIME = :AUDTIME,                            *00371000
                     AUDIT_USER = :AUDUSER,                            *00372000
                     AUDIT_ORG = :AUDORG,                              *00373000
                     BAL_OPT = :BALOPT,                                *00374000
                     AMOUNT_01 = :AMOUNT1,                             *00375000
                     SEC_SVC_01 = :SECSERV1,                           *00376000
                     AMOUNT_02 = :AMOUNT2,                             *00377000
                     SEC_SVC_02 = :SECSERV2,                           *00378000
                     AMOUNT_03 = :AMOUNT3,                             *00379000
                     SEC_SVC_03 = :SECSERV3,                           *00380000
                     AMOUNT_04 = :AMOUNT4,                             *00381000
                     SEC_SVC_04 = :SECSERV4,                           *00382000
                     AMOUNT_05 = :AMOUNT5,                             *00383000
                     SEC_SVC_05 = :SECSERV5,                           *00384000
                     AMOUNT_06 = :AMOUNT6,                             *00385000
                     SEC_SVC_06 = :SECSERV6,                           *00386000
                     AMOUNT_07 = :AMOUNT7,                             *00387000
                     SEC_SVC_07 = :SECSERV7,                           *00388000
                     AMOUNT_08 = :AMOUNT8,                             *00389000
                     SEC_SVC_08 = :SECSERV8,                           *00390000
                     AMOUNT_09 = :AMOUNT9,                             *00391000
                     SEC_SVC_09 = :SECSERV9,                           *00392000
                     AMOUNT_10 = :AMOUNT10,                            *00393000
                     SEC_SVC10 = :SECSER10                             *00394000
                 WHERE CURRENT OF S12UPD0                               00395000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00396000
         BR    14                      RETURN TO CALLER                 00397000
         LTORG                                                          00398000
*                                                                       00399000
**********************************************************************  00400000
* DELETE STATEMENT BY PRIMARY KEY:                                      00401000
*   THIS STATEMENT SUPPORTS THE DELETE VERB.                            00402000
**********************************************************************  00403000
*                                                                       00404000
DELUC0   DS    0H                                                       00405000
         USING DELUC0,12               ESTABLISH BASE REGISTER          00406000
         EXEC  SQL DELETE FROM S12                                     *00407000
                 WHERE CURRENT OF S12UPD0                               00408000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00409000
         BR    14                      RETURN TO CALLER                 00410000
         LTORG                                                          00411000
*                                                                       00412000
**********************************************************************  00413000
* DELETE ALL STATEMENT:                                                 00414000
*   THIS STATEMENT SUPPORTS THE DELETE-FILE VERB.                       00415000
**********************************************************************  00416000
*                                                                       00417000
DELTBL   DS    0H                                                       00418000
         USING DELTBL,12               ESTABLISH BASE REGISTER          00419000
         EXEC  SQL DELETE FROM S12                                      00420000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00421000
         BR    14                      RETURN TO CALLER                 00422000
         LTORG                                                          00423000
*                                                                       00424000
**********************************************************************  00425000
* CLOSE UPDATE CURSOR STATEMENT FOR PRIMARY KEY:                        00426000
*   THIS STATEMENT SUPPORTS THE GET-LOCK VERB.                          00427000
*   IT WILL FOLLOW THE LAST FETCH FROM UPDATE CURSOR STATEMENT          00428000
*     TO CLOSE THE UPDATE CURSOR.                                       00429000
**********************************************************************  00430000
*                                                                       00431000
CLSUC0   DS    0H                                                       00432000
         USING CLSUC0,12               ESTABLISH BASE REGISTER          00433000
         EXEC  SQL CLOSE S12UPD0                                        00434000
         XC    SQWCSUCA,SQWCSUCA       CLEAR CURSOR CLOSE ROUTINE ADDR  00435000
         L     14,SQW@RET              LOAD RETURN ADDRESS              00436000
         BR    14                      RETURN TO CALLER                 00437000
         LTORG                                                          00438000
*                                                                       00439000
**********************************************************************  00440000
* ALTERNATE KEY 1 NOT DEFINED                                           00441000
**********************************************************************  00442000
*                                                                       00443000
SELUC1   DS    0H                                                       00444000
FETUC1   DS    0H                                                       00445000
UPDUC1   DS    0H                                                       00446000
DELUC1   DS    0H                                                       00447000
CLSUC1   DS    0H                                                       00448000
         DC    X'00DEAD01'             FORCE S0C1 ABEND                 00449000
*                                                                       00450000
**********************************************************************  00451000
* ALTERNATE KEY 2 NOT DEFINED                                           00452000
**********************************************************************  00453000
*                                                                       00454000
SELUC2   DS    0H                                                       00455000
FETUC2   DS    0H                                                       00456000
UPDUC2   DS    0H                                                       00457000
